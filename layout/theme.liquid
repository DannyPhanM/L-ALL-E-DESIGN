  </head>
  {% liquid
    assign uppercase_buttons = false
    assign bolder_font_buttons = false
    if settings.typography_preset == 'custom' and settings.regular_button_size == 'default'
      if settings.uppercase_body_s
        assign uppercase_buttons = true
      endif
      if settings.bolder_font_body_s
        assign bolder_font_buttons = true
      endif
    endif
    assign navigations_type = ''
    if settings.typography_preset == 'custom' and settings.navigation_item_size != 'default'
      if settings.navigation_item_size contains 'body_l'
        assign navigations_type = 'navigations-body-l'
      elsif settings.navigation_item_size contains 'body_m'
        assign navigations_type = 'navigations-body-m'
      elsif settings.navigation_item_size contains 'body_s'
        assign navigations_type = 'navigations-body-s'
      elsif settings.navigation_item_size contains 'label'
        assign navigations_type = 'navigations-label'
      endif
    endif
  %} 
  <body 
    id="body" 
    class="body {{ settings.typography_preset }} template-{{ template.name }}
      {%- if settings.select_text_accent_color %} body-accent-selection {% endif -%} 
      {%- if settings.disable_text_selection %}disable-selection {% endif -%} 
      {%- if settings.enable_page_fade_in and settings.page_fade_in_area == 'page' %} fade-in {% endif -%} 
      {%- if template.name == 'index' or template.name == 'collection' %} body--contain {% endif -%}
      {% if settings.prevent_fields_scale %} prevent-fields-scale{% endif %}
      {%- if settings.typography_preset == 'custom' %} custom-type-preset {{ navigations_type }}{% if uppercase_buttons %} buttons-uppercase{% endif %}{% if bolder_font_buttons %} buttons-font-bolder{% endif %} {% endif -%}
       
    "
  >
    {% if settings.allow_transparent_navigation %}<overlap-navigation>{% endif %}
    {% section 'announcement-bar' %}
    {% if settings.menu_drawer_style == 'default' %}
       {% section 'menu-drawer' %}
    {% else %}
      {% section 'mega-menu-drawer' %}
    {% endif %}
    {%- if settings.disable_shopping_functionality == false -%}
      {%- section 'cart-drawer' -%}
    {%- endif -%} 
    
    <div class="theme-content {% if settings.navigation_layout == 'sidebar_based' %}with-sidebars {{ settings.sidebar_template }} {{ settings.sidebar_template }}--{{ settings.white_space }}{% endif %}">
      
      {% if settings.navigation_layout == 'sidebar_based' %}
        {{ 'main-sidebar.css' | asset_url | stylesheet_tag }}
        {% section 'main-sidebar' %}
      {% endif %}
      {% if settings.navigation_layout == 'sidebar_based' and settings.sidebar_template contains 'with_secondary' %}
        {{ 'secondary-sidebar.css' | asset_url | stylesheet_tag }}
        {% section 'secondary-sidebar' %}
      {% endif %}
      <div class="inside-content {% if settings.enable_page_fade_in and settings.page_fade_in_area == 'content' %} fade-in fade-in--content {% endif %}">
            {% if settings.navigation_layout == 'header_based' %}
              {{ 'header.css' | asset_url | stylesheet_tag }}
              {% section 'header' %}
            {% elsif settings.navigation_layout == 'sidebar_based' and settings.show_header %}
              {{ 'header.css' | asset_url | stylesheet_tag }}
              {% section 'secondary-header' %}
            {% endif %}
            {% section 'mobile-header' %}
            <div class="header-group {% if settings.allow_transparent_navigation and settings.allow_mobile_transparent_navigation %} header-group--mobile-overlap-enabled{% endif %}">
              <div class="header-group__sections">
                {% sections 'more-header-sections' %}
              </div>
            </div>
        <main class="main--{{ settings.white_space }} {{ settings.typography_preset }}
          {% if request.page_type == 'policy' %} body-text--{{ settings.typography_preset }}
          {% if settings.typography_preset == 'custom' %}{% if settings.uppercase_heading_xs %} uppercase-heading{% else %} none-uppercase-heading{% endif %}{% if settings.bolder_font_heading_xs %} bolder-font-heading{% else %} none-bolder-font-heading{% endif %}{% if settings.uppercase_body_m %} uppercase{% endif %}{% if settings.bolder_font_body_m %} bolder-font{% endif %}{% endif %}{% if settings.center_text %} main--center{% endif %}{% endif %}">
          {{ content_for_layout }}
        </main>
        {% sections 'footer-group' %}
      </div>
    </div>
    {% if settings.allow_transparent_navigation %}</overlap-navigation>{% endif %}
    <div class="overlay"></div>
    <quick-view-drawer>
      {% style %}
        quick-view-drawer {  
          --layout-text-color:
            {{ settings.layout_text_color.red }},
            {{ settings.layout_text_color.green }}, 
            {{ settings.layout_text_color.blue }}; 
          --layout-background-color:
            {{ settings.layout_background_color.red }},
            {{ settings.layout_background_color.green }},
            {{ settings.layout_background_color.blue }};     
        }
      {% endstyle %}
      <details>
        <summary class="quick-view__summary" tabindex="-1">
        </summary>
        <quick-view class="quick-view">
          <div role="dialog" aria-modal="true" class="quick-view__content" tabindex="-1"></div>
        </quick-view>
      </details>
    </quick-view-drawer>
    <script>
      window.shopUrl = '{{ request.origin }}';
      window.routes = {
        cart_add_url: '{{ routes.cart_add_url }}',
        cart_change_url: '{{ routes.cart_change_url }}',
        cart_update_url: '{{ routes.cart_update_url }}',
        cart_url: '{{ routes.cart_url }}',
        search_url: '{{ routes.search_url }}',
        predictive_search_url: '{{ routes.predictive_search_url }}',
        root_url: '{{ routes.root_url }}'
      };

      window.cartStrings = {
        {% if settings.cart_name == 'cart' %}
          error: `{{ 'sections.cart.cart_error' | t }}`,
          quantityError: `{{ 'sections.cart.cart_quantity_error_html' | t: quantity: '[quantity]' }}`
        {% elsif settings.cart_name == 'bag' %}
          error: `{{ 'sections.cart.bag_error' | t }}`,
          quantityError: `{{ 'sections.cart.bag_quantity_error_html' | t: quantity: '[quantity]' }}`
        {% else %}
          error: `{{ 'sections.cart.basket_error' | t }}`,
          quantityError: `{{ 'sections.cart.basket_quantity_error_html' | t: quantity: '[quantity]' }}`
        {% endif %}
      }

      window.variantStrings = {
        {% if settings.cart_name == 'cart' %}
          addToCart: `{{ 'products.product.add_to_cart' | t }}`,
        {% elsif settings.cart_name == 'bag' %}
          addToCart: `{{ 'products.product.add_to_bag' | t }}`,
        {% else %}
          addToCart: `{{ 'products.product.add_to_basket' | t }}`,
        {% endif %}
        preOrder: `{{ 'products.product.pre_order' | t }}`,
        soldOut: `{{ 'products.product.sold_out' | t }}`,
        unavailable: `{{ 'products.product.unavailable' | t }}`,
        unavailable_with_option: `{{ 'products.product.value_unavailable' | t: option_value: '[value]'}}`,
        customPropertyError: `{{ 'products.product.custom_error' | t }}`,
        fileSizeError: `{{ 'products.product.file_size_error' | t }}`,
        fileRequiredError: `{{ 'products.product.error_message_required' | t }}`
      }

      window.accessibilityStrings = {
        imageAvailable: `{{ 'products.product.media.image_available' | t: index: '[index]' }}`,
        pauseSlideshow: `{{ 'sections.slideshow.pause_slideshow' | t }}`,
        playSlideshow: `{{ 'sections.slideshow.play_slideshow' | t }}`,
        play: `{{ 'accessibility.play' | t }}`,
        pause: `{{ 'accessibility.pause' | t }}`,
        mute: `{{ 'accessibility.mute' | t }}`,
        unmute: `{{ 'accessibility.unmute' | t }}`
      }
    </script>
    <script src="{{ 'quick-view.js' | asset_url }}" defer="defer"></script>
    {% if settings.enable_quick_search %}
      <script src="{{ 'search-modal.js' | asset_url }}" defer="defer"></script>
      <script src="{{ 'predictive-search.js' | asset_url }}" defer="defer"></script>
    {% endif %}
    {% if settings.allow_transparent_navigation %}
      <script src="{{ 'overlap-navigation.js' | asset_url }}" defer="defer"></script>
    {% endif %}
    {% if settings.preload_links %}
      <script src="{{ 'quicklink.umd.js' | asset_url }}" defer="defer"></script>
      <script>
        window.addEventListener('load', () => {
          const waitForQuicklink = () => {
            if (typeof quicklink !== 'undefined') {
              quicklink.listen({
                el: document.body,
                ignores: [
                  uri => {
                    const sameOrigin = new URL(uri, location.href).origin === location.origin;
                    return !sameOrigin;
                  },
                  uri => uri.includes('#'),
                  uri => {
                    const path = new URL(uri, location.href).pathname;
                    return (
                      path.startsWith('/account') ||
                      path.startsWith('/cart') ||
                      path.startsWith('/checkout') ||
                      path.includes('/login')
                    );
                  }
                ]
              });
            } else {
              setTimeout(waitForQuicklink, 50);
            }
          };
          waitForQuicklink();

        });
      </script>
    {% endif %}
  </body>
</html>