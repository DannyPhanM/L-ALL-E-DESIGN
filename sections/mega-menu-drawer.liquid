{{ 'mega-menu-drawer.css' | asset_url | stylesheet_tag }}
{{ 'menus.css' | asset_url | stylesheet_tag }}

{% style %}
#shopify-section-{{ section.id }} {
    {% if section.settings.color_type == 'invert' %}
      --layout-text-color:
        {{ settings.layout_background_color.red }},
        {{ settings.layout_background_color.green }},
        {{ settings.layout_background_color.blue }};
      --layout-background-color:
        {{ settings.layout_text_color.red }},
        {{ settings.layout_text_color.green }}, 
        {{ settings.layout_text_color.blue }};
      {% endif %}
      {% if section.settings.color_type == 'custom' %}
      --layout-text-color:
        {{ section.settings.text_color.red }},
        {{ section.settings.text_color.green }}, 
        {{ section.settings.text_color.blue }};
      --layout-background-color:
        {{ section.settings.background_color.red }},
        {{ section.settings.background_color.green }}, 
        {{ section.settings.background_color.blue }};
    {% endif %}
}
{% endstyle %}


{%- liquid
    assign blocks = section.blocks 
    assign animationDuration = 0
    if settings.animation_duration == '0.3'
        assign animationDuration = 'basic'
    elsif settings.animation_duration == '0.5'
        assign animationDuration = 'sophisticated'
    endif

    if settings.typography_preset == 'avant_garde'
            assign close_weight = settings.accent_icon_weight
            if settings.swap_fonts
                    assign close_weight = settings.base_icon_weight
            endif
    else
            assign close_weight =  settings.base_icon_weight
            if settings.swap_fonts
                    assign close_weight = settings.accent_icon_weight
            endif
    endif
    if settings.typography_preset == 'custom'
        assign close_weight = settings.custom_icon_weight
    endif

    if section.settings.pinned_block != 'none'
    assign last_pinned_block_index = nil
    for block in section.blocks
        if block.type == "country_and_language" and section.settings.pinned_block == 'country_and_language'
            assign last_pinned_block_index = forloop.index0
        endif
        if block.type == "buttons" and section.settings.pinned_block == 'buttons'
            assign last_pinned_block_index = forloop.index0
        endif
        if block.type == "account" and section.settings.pinned_block == 'account'
            assign last_pinned_block_index = forloop.index0
        endif
        if block.type == "context_image" and section.settings.pinned_block == 'context_image'
            assign last_pinned_block_index = forloop.index0
        endif
        if block.type == "miscellaneous" and section.settings.pinned_block == 'miscellaneous'
            assign last_pinned_block_index = forloop.index0
        endif
        if block.type == "store_selector" and section.settings.pinned_block == 'store_selector'
            assign last_pinned_block_index = forloop.index0
        endif
    endfor
endif

assign uppercase_font_heading = false
assign bolder_font_heading = false
if settings.typography_preset == 'custom' and section.settings.heading_size == 'default'
    if settings.uppercase_body_s
        assign uppercase_font_heading = true
    endif
    if settings.bolder_font_body_s
        assign bolder_font_heading = true
    endif
endif
-%}
<menu-drawer>
<div class="menu-drawer mega-menu-drawer hidden color-{{ section.settings.color_type }} 
    {% if section.settings.color_type != 'default' %}no-default-color{% endif %}
    {% if section.settings.drawer_position == 'left' %}menu-drawer--left{% endif %}" 
    id="mega-menu-drawer" 
    hidden
>
    <div class="scroll-area hide-scrollbar">
        <div class="menu-drawer__content 
                    menu-drawer__content--{{ settings.white_space }}
                    menu-drawer__content--{{ animationDuration }}">
            <div class="menu-drawer__header">
            <p class="body-secondary-text--{{ settings.typography_preset }} {% if section.settings.heading_size != 'default' %}{{ section.settings.heading_size }}{% else %}{% if uppercase_font_heading %} uppercase{% endif %}{% if bolder_font_heading %} bolder-font{% endif %}{% endif %}">{{ section.settings.heading }}</p>
            </div>
            {%- for block in blocks -%}
            {%- case block.type -%}
                {%- when 'text' -%}
                    {% if block.settings.text_subheading != blank or block.settings.text_content != blank %}
                        {% liquid
                            assign uppercase_font_subheading = false
                            assign bolder_font_subheading = false
                            assign uppercase_font = false
                            assign bolder_font = false
                            if settings.typography_preset == 'custom' 
                                if block.settings.subheading_size == 'default'
                                    if settings.uppercase_body_s
                                            assign uppercase_font_subheading = true
                                    endif
                                    if settings.bolder_font_body_s
                                            assign bolder_font_subheading = true
                                    endif
                                endif  
                                if block.settings.text_size == 'default'
                                    if settings.uppercase_body_s
                                            assign uppercase_font = true
                                    endif
                                    if settings.bolder_font_body_s
                                            assign bolder_font = true
                                    endif
                                endif  
                            endif
                        %}
                        <div class="block-{{ forloop.index }} text__block{% if block.settings.show_bottom_line %} full-bottom-line{% endif %}" style="--text-horizontal-alignment: {{ block.settings.text_horizontal_alignment }}; --item-index: {{ forloop.index }}; --spacing: {{ block.settings.top_spacing }}; --mobile-spacing: {{ block.settings.mobile_top_spacing }};" {{ block.shopify_attributes }}>
                            {{ 'text.css' | asset_url | stylesheet_tag }}
                            <div class="text__container text__container--{{ block.settings.text_horizontal_alignment }} {% if section.settings.color_type == 'custom' %}color-custom{% elsif section.settings.color_type == 'invert' %}color-invert{% endif %} {% if block.settings.use_accent_color %}color-accent{% endif %}">
                                {% if block.settings.text_subheading != blank %}
                                    <p class="text__heading text__heading--{{ settings.typography_preset }} {% if block.settings.subheading_size != 'default' %}{{ block.settings.subheading_size }}{% else %}{% if uppercase_font_subheading %} uppercase{% endif %}{% if bolder_font_subheading %} bolder-font{% endif %}{% endif %} subheading {% if settings.dim_text %}dim{% endif %}">{{ block.settings.text_subheading }}</p>
                                {% endif %}
                                <div class="text__content richtext {% if settings.enable_animated_underline %}richtext-link--underline{% endif %} text__content--{{ settings.typography_preset }} {% if block.settings.text_size != 'default' %}{{ block.settings.text_size }}{% else %}{% if uppercase_font %} uppercase{% endif %}{% if bolder_font %} bolder-font{% endif %}{% endif %}">{{ block.settings.text_content }}</div>
                            </div>
                        </div>
                    {% endif %}
                {%- when 'search' -%}
                    {%- render 'search', search_settings: block.settings, type: block.type, block_item: block, for_loop: forloop, mega_drawer: true -%}
                {%- when 'main_menu' -%}
                    {% if block.settings.menu != blank %}
                        <div class="main-drawer-menu {% if block.settings.show_lines %}menu-with-lines{% endif %} block-{{ forloop.index }}" style="--spacing: {{ block.settings.top_spacing }}; --mobile-spacing: {{ block.settings.mobile_top_spacing }};" {{ block.shopify_attributes }}>
                            {% render 'drawer-menu', menu: block.settings.menu, horizontal_alignment: block.settings.menu_horizontal_alignment, child_menu: true, show_nested: true, event_type: 'click', menu_settings: block.settings, block: block, blocks: blocks, uppercase_font_heading: uppercase_font_heading, bolder_font_heading: bolder_font_heading  %}
                        </div>
                    {% endif %}
                {%- when 'secondary_menu' -%}
                    {% if block.settings.menu_1 != blank or block.settings.menu_2 != blank %}
                        {% liquid
                            assign uppercase_font_heading = false
                            assign bolder_font_heading = false
                            if settings.typography_preset == 'custom' and block.settings.heading_size == 'default'
                                if settings.uppercase_body_s
                                        assign uppercase_font_heading = true
                                endif
                                if settings.bolder_font_body_s
                                        assign bolder_font_heading = true
                                endif
                            endif
                        %}
                        <div class="block-{{ forloop.index }} menus-block {% if block.settings.show_bottom_line %} full-bottom-line{% endif %} menus-block--{{ block.settings.menu_alignment }}" style="--spacing: {{ block.settings.top_spacing }}; --mobile-spacing: {{ block.settings.mobile_top_spacing }}; --menus-horizontal-alignment: {{ block.settings.menu_horizontal_alignment }}; --item-index: {{ forloop.index }};" {{ block.shopify_attributes }}>
                            <div class="menus menus--secondary menus--{{ block.settings.menu_horizontal_alignment }} menus--column">
                                {% if block.settings.menu_1 != blank %}
                                    <div class="menus__item menus__item--{{ settings.white_space }}">
                                        {% if block.settings.menu_1_heading != blank %}
                                            {% liquid
                                                assign heading_text = block.settings.menu_1_heading
                                                assign final_text = ''
                                                assign split_text_curly = heading_text | split: '{'
                                                for part in split_text_curly
                                                    if part contains '}'
                                                        assign split_curly = part | split: '}'
                                                        assign styled_text = split_curly[0]
                                                        assign after_curly = split_curly[1]
                                                        assign final_text = final_text | append: '<span class="styled">' | append: styled_text | append: '</span>' | append: after_curly
                                                    else
                                                        assign final_text = final_text | append: part
                                                    endif
                                                endfor
                                              %}
                                            <p class="menus__item-title custom-heading menus__item-title--{{ settings.typography_preset }} {% if block.settings.heading_size != 'default' %}{{ block.settings.heading_size }}{% else %}{% if uppercase_font_heading %} uppercase{% endif %}{% if bolder_font_heading %} bolder-font{% endif %}{% endif %} subheading {% if settings.dim_text %}dim{% endif %}">{{ final_text }}</p>
                                        {% endif %}
                                        {% if block.settings.menu_1_heading == blank and block.settings.show_menu_title and block.settings.menu_1.title %}
                                            <p class="menus__item-title menus__item-title--{{ settings.typography_preset }} {% if block.settings.heading_size != 'default' %}{{ block.settings.heading_size }}{% else %}{% if uppercase_font_heading %} uppercase{% endif %}{% if bolder_font_heading %} bolder-font{% endif %}{% endif %} subheading {% if settings.dim_text %}dim{% endif %}">{{ block.settings.menu_1.title }}</p>
                                        {% endif %}
                                        {% assign menu_id = block.id | append: '1' %}
                                        {%- render 'menu', menu_settings: block.settings, menu_settings_id: block.settings.menu_1, block_item: block, type: block.type, menu_id: menu_id, mega_drawer: true -%}
                                    </div>
                                {% endif %}
                                {% if block.settings.menu_2 != blank %}
                                    <div class="menus__item menus__item--{{ settings.white_space }}">
                                        {% if block.settings.menu_2_heading != blank %}
                                            {% liquid
                                                assign heading_text = block.settings.menu_2_heading
                                                assign final_text = ''
                                                assign split_text_curly = heading_text | split: '{'
                                                for part in split_text_curly
                                                    if part contains '}'
                                                        assign split_curly = part | split: '}'
                                                        assign styled_text = split_curly[0]
                                                        assign after_curly = split_curly[1]
                                                        assign final_text = final_text | append: '<span class="styled">' | append: styled_text | append: '</span>' | append: after_curly
                                                    else
                                                        assign final_text = final_text | append: part
                                                    endif
                                                endfor
                                              %}
                                            <p class="menus__item-title custom-heading menus__item-title--{{ settings.typography_preset }} {% if block.settings.heading_size != 'default' %}{{ block.settings.heading_size }}{% else %}{% if uppercase_font_heading %} uppercase{% endif %}{% if bolder_font_heading %} bolder-font{% endif %}{% endif %} subheading {% if settings.dim_text %}dim{% endif %}">{{ final_text }}</p>
                                        {% endif %}
                                        {% if block.settings.menu_2_heading == blank and block.settings.show_menu_title and block.settings.menu_2.title %}
                                            <p class="menus__item-title menus__item-title--{{ settings.typography_preset }} {% if block.settings.heading_size != 'default' %}{{ block.settings.heading_size }}{% else %}{% if uppercase_font_heading %} uppercase{% endif %}{% if bolder_font_heading %} bolder-font{% endif %}{% endif %} subheading {% if settings.dim_text %}dim{% endif %}">{{ block.settings.menu_2.title }}</p>
                                        {% endif %}
                                        {% assign menu_id = block.id | append: '2' %}
                                        {%- render 'menu', menu_settings: block.settings, menu_settings_id: block.settings.menu_2, block_item: block, type: block.type, menu_id: menu_id, mega_drawer: true -%}
                                    </div>
                                {% endif %}
                            </div>
                        </div>
                    {% endif %}
                {%- when 'miscellaneous' -%}
                    {%- render 'miscellaneous', miscellaneous_settings: block.settings, type: block.type, block: block, for_loop: forloop, mega_drawer: true, last_pinned_block_index: last_pinned_block_index -%}
                {%- when 'line' -%}
                    <div class="block-{{ forloop.index }} line-container line-container--{{ block.id }}" data-block-index="{{ forloop.index0 }}" {{ block.shopify_attributes }} style=" --item-index: {{ forloop.index }};">
                        {% style %}
                            .line-container--{{ block.id }} {
                                --top-spacing: {{ block.settings.top_spacing }};
                                --bottom-spacing: {{ block.settings.bottom_spacing }};
                                padding: calc(var(--menu-drawer-spacing) * var(--top-spacing)) 0 calc(var(--menu-drawer-spacing) * var(--bottom-spacing));
                            }
                        {% endstyle %} 
                        <span class="line {% if section.settings.color_type == 'custom' %}color-custom{% elsif section.settings.color_type == 'invert' %}color-invert{% endif %}"></span>
                    </div>
                {%- when 'newsletter' -%}
                    {%- render 'newsletter', newsletter_settings: block.settings, block_item: block, for_loop: forloop, mega_drawer: true -%}
                {%- when 'social_media' -%}
                    {%- render 'social-media', social-media_settings: block.settings, type: block.type, block_item: block, for_loop: forloop, section_settings: section.settings, mega_drawer: true -%}
                {%- when 'image' -%}
                    {%- render 'image', image_settings: block.settings, type: block.type, block_item: block, for_loop: forloop -%}
                {%- when 'empty_space' -%}
                    <div class="block-{{ forloop.index }} empty-space" {{ block.shopify_attributes }} style="--item-index: {{ forloop.index }}; --desktop-height: {{ block.settings.desktop_height }}px; --mobile-height: {{ block.settings.mobile_height }}px;"></div>
                {%- when 'store_selector' -%}
                    <div class="store-selector-container {% if section.settings.pinned_block == 'store_selector' and forloop.index0 == last_pinned_block_index %} pinned-block{% endif %} {% if block.settings.show_bottom_line %} full-bottom-line{% endif %}" 
                        style="--horizontal-alignment: {{ block.settings.store_selector_horizontal_alignment }}; --item-index: {{ forloop.index }}; --spacing: {{ block.settings.top_spacing }}; --mobile-spacing: {{ block.settings.mobile_top_spacing }};" 
                        data-block-index="{{ forloop.index0 }}" 
                        {{ block.shopify_attributes }}
                    >
                        {% render 'store-selector', store_selector_settings: block.settings %}
                    </div>
                {%- when 'country_and_language' -%}
                    {%- render 'country-and-language', country-and-language_settings: block.settings, type: block.type, for_loop: forloop, block_item: block, mega_drawer: true, last_pinned_block_index: last_pinned_block_index -%}
                {%- when 'contacts' -%}
                    {%- render 'contacts', contacts_settings: block.settings, for_loop: forloop, block_item: block, mega_drawer: true -%}
                {%- when 'context_image' -%}
                    <div class="context-image-container context-image-container--{{ block.id }}{% if block.settings.show_bottom_line %} full-bottom-line{% endif %}{% if section.settings.pinned_block == 'context_image' and forloop.index0 == last_pinned_block_index %} pinned-block{% endif %}" style="--item-index: {{ forloop.index }}; --spacing: {{ block.settings.top_spacing }}; --mobile-spacing: {{ block.settings.mobile_top_spacing }};" {{ block.shopify_attributes }}>
                        {%- render 'context-image', block_or_section: block, block: block -%}
                    </div>
                {%- when 'buttons' -%}
                    <div class="navigation-buttons navigation-buttons--{{ block.id }}{% if section.settings.pinned_block == 'buttons' and forloop.index0 == last_pinned_block_index %} pinned-block{% endif %}{% if block.settings.show_bottom_line %} full-bottom-line{% endif %}" data-block-index="{{ forloop.index0 }}" 
                        style="--item-index: {{ forloop.index }}; --spacing: {{ block.settings.top_spacing }}; --mobile-spacing: {{ block.settings.mobile_top_spacing }};" {{ block.shopify_attributes }}>
                        {% if block.settings.heading != blank %}
                            {% liquid
                                assign uppercase_font_subheading = false
                                assign bolder_font_subheading = false
                                if settings.typography_preset == 'custom' and block.settings.subheading_size == 'default'
                                    if settings.uppercase_body_s
                                        assign uppercase_font_subheading = true
                                    endif
                                    if settings.bolder_font_body_s
                                        assign bolder_font_subheading = true
                                    endif 
                                endif
                            %}
                            <p class="navigation-buttons__title navigation-buttons__title--{{ settings.typography_preset }} {% if block.settings.subheading_size != 'default' %}{{ block.settings.subheading_size }}{% else %}{% if uppercase_font_subheading %} uppercase{% endif %}{% if bolder_font_subheading %} bolder-font{% endif %}{% endif %} subheading {% if settings.dim_text %}dim{% endif %} ">{{ block.settings.heading }}</p>
                        {% endif %}
                        <div class="navigation-buttons__wrapper">
                            {%- render 'button-with-icon', button_settings: block.settings, button_label: block.settings.button_label_1, button_link: block.settings.button_link_1, button_style: block.settings.button_style_1, target_blank: block.settings.target_blank_1, icon: block.settings.icon_1, block: block, mega_drawer: true -%}
                            {%- render 'button-with-icon', button_settings: block.settings, button_label: block.settings.button_label_2, button_link: block.settings.button_link_2, button_style: block.settings.button_style_2, target_blank: block.settings.target_blank_2, icon: block.settings.icon_2, block: block, mega_drawer: true -%}
                        </div>
                    </div>
                {%- when 'custom_liquid' -%}
                    {%- render 'custom-liquid', block: block, mega_drawer: true -%}
                {% when 'account' %}
                    {%- if shop.customer_accounts_enabled -%}
                        <div class="block-account block-{{ forloop.index }} {% if block.settings.show_bottom_line %}full-bottom-line{% endif %}{% if section.settings.pinned_block == 'account' and forloop.index0 == last_pinned_block_index %} pinned-block{% endif %}" style="--spacing: {{ block.settings.top_spacing }}; --mobile-spacing: {{ block.settings.mobile_top_spacing }};" {{ block.shopify_attributes }}>
                            {% style %}
                                .block-account .account__icon {
                                    height: {{ 24 | times: settings.desktop_font_scale | divided_by: 100.0 | round | at_least: 24 }}px;
                                }
                                .custom-type-preset .block-account .account__icon {
                                    height: max(24px, calc(var(--body-s-font-size) * var(--body-s-line-height)));
                                }
                                .custom-type-preset.navigations-body-l .block-account .account__icon {
                                    height: max(24px, calc(var(--body-l-font-size) * var(--body-l-line-height)));
                                }
                                .custom-type-preset.navigations-body-m .block-account .account__icon {
                                    height: max(24px, calc(var(--body-m-font-size) * var(--body-m-line-height)));
                                }
                                .custom-type-preset.navigations-label .block-account .account__icon {
                                    height: max(24px, calc(var(--label-font-size) * var(--label-line-height)));
                                }
                                @media screen and (max-width: 920px) {
                                    .block-account .account__icon {
                                        height: {{ 24 | times: settings.mobile_font_scale | divided_by: 100.0 | round | at_least: 24 }}px;
                                    }
                                }
                            {% endstyle %}
                            {% liquid
                                assign uppercase_font = false
                                assign bolder_font = false
                                if settings.typography_preset == 'custom' and settings.navigation_item_size == 'default'
                                    if settings.uppercase_body_s
                                        assign uppercase_font = true
                                    endif
                                    if settings.bolder_font_body_s
                                        assign bolder_font = true
                                    endif
                                endif
                            %}
                            <a href="{{ routes.account_url }}" class="account-container--{{ settings.typography_preset }} {% if settings.navigation_item_size != 'default' and settings.typography_preset == 'custom' %}{{ settings.navigation_item_size }}{% else %}{% if uppercase_font %} uppercase{% endif %}{% if bolder_font %} bolder-font{% endif %}{% endif %} account-link {% if block.settings.horizontal_alignment == 'center' %} center{% endif %}" aria-label="{{ 'customer.account.go_to_account' | t }}">
                                <div class="account__wrapper">
                                    {% if block.settings.show_icon %}
                                        <span class="account__icon">
                                            {% if close_weight == 'regular' %}
                                                {% render 'icon', icon: 'account-regular' %}
                                            {% elsif close_weight == 'medium' %}
                                                {% render 'icon', icon: 'account-medium' %}
                                            {% elsif close_weight == 'bold' %}
                                                {% render 'icon', icon: 'account-bold' %}
                                            {% endif %}
                                        </span>
                                    {% endif %}
                                    {% if customer %}<span class="account__title">{{ 'customer.account.title' | t }}</span>{% else %}<span class="account__title">{{ 'customer.account.create_account' | t }}</span>{% endif %}
                                </div>
                            </a>
                        </div>
                    {%- endif -%}
            {%- endcase -%}
        {%- endfor -%}
        </div>
        <div class="button-wrapper menu-drawer__close {% if settings.enable_buttons_zoom %}buttons-zoom{% endif %}">
            <button type="button" class="button-close">
            {% if close_weight == 'light' %}
                {% render 'icon', icon: 'cross-light' %}
            {% elsif close_weight == 'regular' %}
                {% render 'icon', icon: 'cross-regular' %}
            {% elsif close_weight == 'medium' %}
                {% render 'icon', icon: 'cross-medium' %}
            {% elsif close_weight == 'bold' %}
                {% render 'icon', icon: 'cross-bold' %}
            {% endif %}
            </button>
        </div>
    </div>
</div>
</menu-drawer>

{% schema %}
{
    "name": "t:sections.menu-drawer.name",
    "class": "section-menu-drawer",
    "settings": [
        {
            "type": "paragraph",
            "content": "t:sections.mega-menu-drawer.settings.paragraph__1.content"
        },
        {
            "type": "select",
            "id": "drawer_position",
            "options": [
                {
                    "value": "left",
                    "label": "t:sections.mega-menu-drawer.settings.drawer_position.options__1.label"
                },
                {
                    "value": "right",
                    "label": "t:sections.mega-menu-drawer.settings.drawer_position.options__2.label"
                }
            ],
            "default": "right",
            "label": "t:sections.mega-menu-drawer.settings.drawer_position.label"
        },
        {
            "type": "text",
            "id": "heading",
            "default": "Menu",
            "label": "t:sections.all.labels.heading.label"
        },
        {
            "type": "select",
            "id": "heading_size",
            "label": "t:sections.all.size.heading_size.label",
            "options": [
              {
                "value": "default",
                "label": "t:sections.all.size.options__1.label"
              },
              {
                  "value": "heading_xl",
                  "label": "t:sections.all.size.options__2.label"
              },
              {
                  "value": "heading_xl_uppercase",
                  "label": "t:sections.all.size.options__3.label"
              },
              {
                  "value": "heading_l",
                  "label": "t:sections.all.size.options__4.label"
              },
              {
                  "value": "heading_l_uppercase",
                  "label": "t:sections.all.size.options__5.label"
              },
              {
                  "value": "heading_m",
                  "label": "t:sections.all.size.options__6.label"
              },
              {
                  "value": "heading_m_uppercase",
                  "label": "t:sections.all.size.options__7.label"
              },
              {
                  "value": "heading_s",
                  "label": "t:sections.all.size.options__8.label"
              },
              {
                  "value": "heading_s_uppercase",
                  "label": "t:sections.all.size.options__9.label"
              },
              {
                  "value": "heading_xs",
                  "label": "t:sections.all.size.options__10.label"
              },
              {
                  "value": "heading_xs_uppercase",
                  "label": "t:sections.all.size.options__11.label"
              },
              {
                  "value": "body_l",
                  "label": "t:sections.all.size.options__12.label"
              },
              {
                  "value": "body_l_uppercase",
                  "label": "t:sections.all.size.options__13.label"
              },
              {
                  "value": "body_m",
                  "label": "t:sections.all.size.options__14.label"
              }, 
              {
                  "value": "body_m_uppercase",
                  "label": "t:sections.all.size.options__15.label"
              }, 
