{% liquid
  assign settings_page_width = settings.max_page_width | append: 'px'
  if settings.max_page_width == 'full_width'
    assign settings_page_width = '100vw'
  endif
%}

<section class="section product-overview-section main-product" data-section="{{ section.id }}">
  {%- liquid 
    if section.settings.focal_point == '20% 0'
      assign vertical_aligment = 'vertical_top'
      assign horizontal_aligment = 'horizontal_left'
    elsif section.settings.focal_point == 'top center'
      assign vertical_aligment = 'vertical_top'
      assign horizontal_aligment = 'horizontal_center'
    elsif section.settings.focal_point == '80% 0'
      assign vertical_aligment = 'vertical_top'
      assign horizontal_aligment = 'horizontal_right'
    elsif section.settings.focal_point == '20% 50%'
      assign vertical_aligment = 'vertical_center'
      assign horizontal_aligment = 'horizontal_left'
    elsif section.settings.focal_point == 'center center'
      assign vertical_aligment = 'vertical_center'
      assign horizontal_aligment = 'horizontal_center'
    elsif section.settings.focal_point == '80% 50%'
      assign vertical_aligment = 'vertical_center'
      assign horizontal_aligment = 'horizontal_right'
    elsif section.settings.focal_point == '20% 100%'
      assign vertical_aligment = 'vertical_bottom'
      assign horizontal_aligment = 'horizontal_left'
    elsif section.settings.focal_point == 'bottom center'
      assign vertical_aligment = 'vertical_bottom'
      assign horizontal_aligment = 'horizontal_center'
    elsif section.settings.focal_point == '80% 100%'
      assign vertical_aligment = 'vertical_bottom'
      assign horizontal_aligment = 'horizontal_right'
    endif
  -%}
  <div class="ordinal-section ordinal-section--{{ settings.white_space }} {% if settings.show_lines %}ordinal-section--lines{% endif %}  {% if settings.max_page_width == '1400' or settings.max_page_width == '1200' %}page-width {% endif %} page-width--{{ settings.max_page_width }}  section-padding--{{ settings.white_space }}">
    <div class="product product--{{ section.settings.desktop_gallery_size }} product--{{ section.settings.desktop_gallery_style }} product--{{ settings.white_space }} {% if settings.show_lines %}product--lines{% endif %} {% if section.settings.swap_position and section.settings.desktop_gallery_size != 'full_width' %}product--reverse{% endif %}{% if section.settings.enable_image_zoom and section.settings.image_zoom_style == 'zoom-in' %} product-with-image-zoom-in{% endif %}">
      <product-recently-viewed data-product-id="{{ product.id }}" class="hidden-block"></product-recently-viewed>
    <div class="grid__item product__media-wrapper">
        {% render 'custom-size-popup', custom_size_image: section.settings.custom_size_image %}
        <media-gallery id="MediaGallery-{{ section.id }}" role="region" {% if section.settings.enable_sticky_content %} class="container--sticky"{% endif %} aria-label="{{ 'products.product.media.gallery_viewer' | t }}" data-desktop-layout="{{ section.settings.desktop_gallery_style }}">
          <div id="GalleryStatus-{{ section.id }}" class="visually-hidden" role="status"></div>
          <div class="slider-block slider-container-js {% if section.settings.desktop_gallery_style == 'side_thumbnails' %}slider-block--row{% else %}slider-block--column{% endif %}">
            <slider-component id="GalleryViewer-{{ section.id }}" class="slider-mobile-gutter slider-mobile-gutter--large" data-enable-autoplay="{{ section.settings.enable_autoplay }}">
              <a class="skip-to-content-link button visually-hidden quick-add-hidden" href="#ProductInfo-{{ section.id }}">
                {{ "accessibility.skip_to_product_info" | t }}
              </a>
              <div class="slider__viewport">
                <ul id="Slider-Gallery-{{ section.id }}" class="slider__grid product__media-list {{ section.settings.desktop_gallery_style }} {% if section.settings.desktop_gallery_size == 'full_width' %}full-width{% endif %} {% if product.media.size == 1 %}one_column{% endif %} contains-media {% if section.settings.desktop_gallery_style == 'one_column_grid' or section.settings.desktop_gallery_style == 'two_columns_grid' %}grid grid--peek{% else %}{% if section.settings.image_ratio != 'original' %}ratio-{{ section.settings.image_ratio }}{% endif %}{% endif %} list-unstyled product-slider slider--mobile slider-main--{{ section.settings.image_ratio }} {{ vertical_aligment }} {{ horizontal_aligment }}{% if section.settings.show_media_attached_to_variant %} variant-images{% endif %}{% if section.settings.organize_images or section.settings.first_image_instead_of_variant %} organize_images{% endif %}" role="list" data-count="1" data-count-mobile="1">
                  {%- liquid
                    assign variant_images = product.images | where: 'attached_to_variant?', true | map: 'src'
                    assign variant_alt = product.selected_or_first_available_variant.image.alt | downcase
                    assign media_count = product.media.size
                    if section.settings.show_media_attached_to_variant and variant_images.size > 0
                      assign media_count = variant_images.size
                    endif
                    if section.settings.desktop_gallery_size == 'large'
                      assign media_width = 0.65
                    elsif section.settings.desktop_gallery_size == 'medium'
                      assign media_width = 0.55
                    elsif section.settings.desktop_gallery_size == 'small'
                      assign media_width = 0.45
                    endif
                  -%}
                  {%- if product.media.size < 1 and settings.product_placeholder != blank -%}
                    <li tabindex="0" class="product__placeholder lazy-image-container product__media-item--{{ section.settings.image_ratio }} ratio-{{ section.settings.image_ratio }}">
                      {% comment %} theme-check-disable ImgLazyLoading {% endcomment %}
                      {% if settings.images_loading != 'disable' %}
                        <figure class="lazy-image lazy-image--{{ settings.images_loading }}" style="--desktop-asp-rat: {{ settings.product_placeholder.aspect_ratio }}">
                      {% endif %}
                      <img
                        srcset="{% if settings.product_placeholder.width >= 493 %}{{ settings.product_placeholder | image_url: width: 493 }} 493w,{% endif %}
                          {% if settings.product_placeholder.width >= 600 %}{{ settings.product_placeholder | image_url: width: 600 }} 600w,{% endif %}
                          {% if settings.product_placeholder.width >= 713 %}{{ settings.product_placeholder | image_url: width: 713 }} 713w,{% endif %}
                          {% if settings.product_placeholder.width >= 823 %}{{ settings.product_placeholder | image_url: width: 823 }} 823w,{% endif %}
                          {% if settings.product_placeholder.width >= 990 %}{{ settings.product_placeholder | image_url: width: 990 }} 990w,{% endif %}
                          {% if settings.product_placeholder.width >= 1100 %}{{ settings.product_placeholder | image_url: width: 1100 }} 1100w,{% endif %}
                          {% if settings.product_placeholder.width >= 1206 %}{{ settings.product_placeholder | image_url: width: 1206 }} 1206w,{% endif %}
                          {% if settings.product_placeholder.width >= 1346 %}{{ settings.product_placeholder | image_url: width: 1346 }} 1346w,{% endif %}
                          {% if settings.product_placeholder.width >= 1426 %}{{ settings.product_placeholder | image_url: width: 1426 }} 1426w,{% endif %}
                          {% if settings.product_placeholder.width >= 1646 %}{{ settings.product_placeholder | image_url: width: 1646 }} 1646w,{% endif %}
                          {% if settings.product_placeholder.width >= 1946 %}{{ settings.product_placeholder | image_url: width: 1946 }} 1946w,{% endif %}
                          {{ settings.product_placeholder | image_url }} {{ settings.product_placeholder.width }}w"
                        sizes="(min-width: {{ settings_page_width }}) calc(({{ settings_page_width }} - 100px) * {{ media_width }}), (min-width: 990px) calc({{ media_width | times: 100 }}vw - 10rem), (min-width: 750px) calc((100vw - 11.5rem) / 2), calc(100vw - 4rem)"
                        class="banner__image banner__image--mobile{% if section.settings.enable_image_zoom and section.settings.image_zoom_style == 'zoom-in' %} image-magnify-hover{% endif %}"
                        src="{{ settings.product_placeholder | image_url: width: 1946 }}"
                        {% if settings.images_loading != 'disable' %}
                          loading="lazy"
                          onload="this.parentNode.classList.add('lazyloaded');"
                        {% else %}
                            {% if section.index > 2 %}loading="lazy"{% endif %}
                        {% endif %}
                        draggable="false"
                        alt="{{ settings.product_placeholder.alt | escape }}"
                        width="{{ settings.product_placeholder.width }}"
                        height="{{ settings.product_placeholder.width | divided_by: settings.product_placeholder.aspect_ratio | ceil }}"
                        {% if settings.disable_right_click %}oncontextmenu="return false"{% endif %}
                      >
                      {% if settings.images_loading != 'disable' %}
                        {% if settings.images_loading != 'fade_scale' %}
                        <div class="lazy-image__preloader lazy-image__preloader--full lazy-image__preloader-{{ settings.images_loading }}" aria-hidden="true">
                            <img 
                            alt="{{ settings.product_placeholder.alt | escape }}" 
                            src="{{ settings.product_placeholder | image_url: width: 10 }}"
                            draggable="false"
                            width="10" 
                            height="10">
                        </div>
                        {% endif %}
                        </figure>
                      {% endif %}
                        {% if section.settings.enable_image_zoom and section.settings.image_zoom_style == 'zoom-in' %}
                          <div class="image-zoom-icon zoom">
                            {%- liquid 
                              if settings.typography_preset == 'avant_garde'
                                  assign weight = settings.accent_icon_weight
                                  if settings.swap_fonts
                                      assign weight = settings.base_icon_weight
                                  endif
                              else 
                                  assign weight =  settings.base_icon_weight
                                  if settings.swap_fonts
                                      assign weight = settings.accent_icon_weight
                                  endif
                              endif
                              if settings.typography_preset == 'custom'
                                  assign weight = settings.custom_icon_weight
                              endif
                            -%}
                            {% if weight == 'regular' %}
                                {% render 'icon-accordion-2', icon: 'zoom-out-regular' %}
                            {% elsif weight == 'medium' %}
                                {% render 'icon-accordion-2', icon: 'zoom-out-medium' %}
                            {% elsif weight == 'bold' %}
                                {% render 'icon-accordion-2', icon: 'zoom-out-bold' %}
                            {% endif %}
                          </div>
                          {% endif %}
                      {% comment %} theme-check-enable ImgLazyLoading {% endcomment %}
                        {% if section.settings.enable_image_zoom and section.settings.image_zoom_style != 'zoom-in' %}
                          {%- liquid 
                            if settings.typography_preset == 'avant_garde'
                                assign weight = settings.accent_icon_weight
                                if settings.swap_fonts
                                    assign weight = settings.base_icon_weight
                                endif
                            else 
                                assign weight =  settings.base_icon_weight
                                if settings.swap_fonts
                                    assign weight = settings.accent_icon_weight
                                endif
                            endif
                            if settings.typography_preset == 'custom'
                                  assign weight = settings.custom_icon_weight
                              endif
                          -%}
                          {% if product.media.media_type == 'image' %}
                            <div class="zoom_icon">
                                  {% if weight == 'regular' %}
                                      {% render 'icon-accordion-2', icon: 'zoom-in-regular' %}
                                  {% elsif weight == 'medium' %}
                                      {% render 'icon-accordion-2', icon: 'zoom-in-medium' %}
                                  {% elsif weight == 'bold' %}
                                      {% render 'icon-accordion-2', icon: 'zoom-in-bold' %}
                                  {% endif %}
                            </div>
                          {% endif %}
                        {% endif %}
                    </li>
                  {%- endif -%}
                  {% liquid 
                    assign admin_order = false
                    if section.settings.organize_images
                      assign admin_order = true
                    endif
                    if section.settings.first_image_instead_of_variant
                      assign admin_order = true
                    endif
                  %}
                  {% if admin_order == false %}
                    {%- if product.selected_or_first_available_variant.featured_media != nil -%}
                      {% liquid 
                        assign featured_media = product.selected_or_first_available_variant.featured_media
                        if featured_media.alt contains '(' or featured_media.alt contains ')' 
                          assign featured_media_alt = featured_media.alt | downcase | split: '(' | last | split: ')' | first | prepend: '(' | append: ')'
                        else
                          assign featured_media_alt = featured_media.alt | downcase
                        endif
                        if featured_media.alt contains '(' or featured_media.alt contains ')'
                          assign general_media = false 
                        else
                          if section.settings.show_extra_images 
                            assign general_media = true
                          endif
                        endif
                      %}
                      <li id="Slide-{{ section.id }}-{{ featured_media.id }}" tabindex="0" class="product__media-item product__media-item--{{ section.settings.image_ratio }} lazy-image-container ratio-{{ section.settings.image_ratio }} {% if section.settings.show_media_attached_to_variant and variant_images.size > 0 %}product__media-item--hide{% endif %} snap-align grid__item slider__slide is-active{% if featured_media.media_type != 'image' or general_media %} product__media-item--show product__media-item--variant-alt product__media-item--full{% endif %}{% if section.settings.show_media_attached_to_variant and variant_alt contains featured_media_alt  %} product__media-item--variant-alt{% endif %}{% if section.settings.show_media_attached_to_variant and variant_images contains featured_media.src  %} product__media-item--variant{% endif %}" data-media-id="{{ section.id }}-{{ featured_media.id }}" data-product-media-id="{{ featured_media.id }}" data-media-alt="{{ featured_media_alt }}">
                        {%- assign media_position = 1 -%}
                        {% render 'product-thumbnail', media: featured_media, position: media_position, loop: section.settings.enable_video_looping, controls: enable_video_controls, autoplay: section.settings.enable_autoplay,  modal_id: section.id, xr_button: true, media_width: media_width, lazy_load: false %}
                        {% if section.settings.enable_image_zoom and section.settings.image_zoom_style != 'zoom-in' %}
                          {%- liquid 
                            if settings.typography_preset == 'avant_garde'
                                assign weight = settings.accent_icon_weight
                                if settings.swap_fonts
                                    assign weight = settings.base_icon_weight
                                endif
                            else 
                                assign weight =  settings.base_icon_weight
                                if settings.swap_fonts
                                    assign weight = settings.accent_icon_weight
                                endif
                            endif
                            if settings.typography_preset == 'custom'
                                  assign weight = settings.custom_icon_weight
                              endif
                          -%}
                          {% if featured_media.media_type == 'image' %}
                            <div class="zoom_icon">
                                  {% if weight == 'regular' %}
                                      {% render 'icon-accordion-2', icon: 'zoom-in-regular' %}
                                  {% elsif weight == 'medium' %}
                                      {% render 'icon-accordion-2', icon: 'zoom-in-medium' %}
                                  {% elsif weight == 'bold' %}
                                      {% render 'icon-accordion-2', icon: 'zoom-in-bold' %}
                                  {% endif %}
                            </div>
                          {% endif %}
                        {% endif %}
                      </li>
                    {%- endif -%}
                  {% endif %}
                  
                  {%- for media in product.media -%}
                      {% liquid 
                        if media.alt contains '(' or media.alt contains ')' 
                          assign media_alt = media.alt | downcase | split: '(' | last | split: ')' | first | prepend: '(' | append: ')' 
                        else 
                          assign media_alt = media.alt | downcase
                        endif
                        assign admin_order = false
                        if section.settings.organize_images
                          assign admin_order = true
                        endif
                        if section.settings.first_image_instead_of_variant
                          assign admin_order = true
                        endif
                        if media.alt contains '(' and media.alt contains ')'
                          assign general_media = false
                        else
                          if section.settings.show_extra_images 
                            assign general_media = true
                          endif
                        endif
                      %}
                      {% if admin_order == false %}
                        {% unless media.id == product.selected_or_first_available_variant.featured_media.id %}
                          <li id="Slide-{{ section.id }}-{{ media.id }}" tabindex="0" class="{% if media == product.selected_or_first_available_variant.image %}is-active{% endif %} product__media-item product__media-item--{{ section.settings.image_ratio }} lazy-image-container ratio-{{ section.settings.image_ratio }} {% if section.settings.show_media_attached_to_variant and variant_images.size > 0 %}product__media-item--hide{% endif %} snap-align grid__item slider__slide{% if product.selected_or_first_available_variant.featured_media == null and forloop.index == 1 %} is-active{% endif %}{% if media.media_type != 'image' or general_media %} product__media-item--show product__media-item--variant-alt product__media-item--full{% endif %}{% if section.settings.show_media_attached_to_variant and variant_alt contains media_alt %} product__media-item--variant-alt{% endif %}{% if section.settings.show_media_attached_to_variant and variant_images contains media.src %} product__media-item--variant{% endif %}{% if product.selected_or_first_available_variant.featured_media == blank %} product__media-item--show{% endif %}" data-media-id="{{ section.id }}-{{ media.id }}" data-product-media-id="{{ media.id }}" data-media-alt="{{ media_alt }}">
                            {%- liquid
                              assign media_position = media_position | default: 0 | plus: 1
                              assign lazy_load = false
                              if media_position > 1
                                assign lazy_load = true
                              endif
                              render 'product-thumbnail', media: media, position: media_position, loop: section.settings.enable_video_looping, controls: enable_video_controls, autoplay: section.settings.enable_autoplay, modal_id: section.id, xr_button: true, media_width: media_width, lazy_load: lazy_load
                            -%}
                            {% if section.settings.enable_image_zoom and section.settings.image_zoom_style != 'zoom-in' %}
                              {%- liquid 
                                if settings.typography_preset == 'avant_garde'
                                    assign weight = settings.accent_icon_weight
                                    if settings.swap_fonts
                                        assign weight = settings.base_icon_weight
                                    endif
                                else 
                                    assign weight =  settings.base_icon_weight
                                    if settings.swap_fonts
                                        assign weight = settings.accent_icon_weight
                                    endif
                                endif
                                if settings.typography_preset == 'custom'
                                  assign weight = settings.custom_icon_weight
                              endif
                              -%}
                              {% if media.media_type == 'image' %}
                                <div class="zoom_icon">
                                      {% if weight == 'regular' %}
