                                        {% endif %}
                                        <p class="product__inventory-status input--{{ settings.typography_preset }} {% if block.settings.text_size != 'default' %}{{ block.settings.text_size }}{% else %}{% if settings.typography_preset == 'custom' %}{% if settings.uppercase_body_s %} uppercase{% endif %}{% if settings.bolder_font_body_s %} bolder-font{% endif %}{% endif %}{% endif %}">{{ 'products.product.inventory.in_stock_label' | t: count: product.selected_or_first_available_variant.inventory_quantity }}</p>
                                        {% if block.settings.show_bar %}
                                          <progress-bar class="progress-bar"></progress-bar>
                                        {% endif %}
                                      </li>
                                    {% endunless %}
                                  {%- elsif inventory_threshold >= inventory_quantity and inventory_quantity >= 0 and product.selected_or_first_available_variant.inventory_management == 'shopify' -%}
                                    <li class="product__inventory-item product__inventory-item--low" style="--low-inventory-color: {{ block.settings.low_inventory_color.red }}, {{ block.settings.low_inventory_color.green }}, {{ block.settings.low_inventory_color.blue }};">
                                      {% if block.settings.show_label %}
                                        <p class="product__inventory-label label--{{ settings.typography_preset }} {% if block.settings.label_size != 'default' %}{{ block.settings.label_size }}{% else %}{% if settings.typography_preset == 'custom' %}{% if settings.uppercase_body_s %} uppercase{% endif %}{% if settings.bolder_font_body_s %} bolder-font{% endif %}{% endif %}{% endif %} {% if settings.dim_text %}dim{% endif %}">{{ 'products.product.inventory.inventory_label' | t }}</p>
                                      {% endif %}
                                      <p class="product__inventory-status input--{{ settings.typography_preset }} {% if block.settings.text_size != 'default' %}{{ block.settings.text_size }}{% else %}{% if settings.typography_preset == 'custom' %}{% if settings.uppercase_body_s %} uppercase{% endif %}{% if settings.bolder_font_body_s %} bolder-font{% endif %}{% endif %}{% endif %}">{{ 'products.product.inventory.stock_label_html' | t: count: product.selected_or_first_available_variant.inventory_quantity }}</p>
                                      {% if block.settings.show_bar %}
                                        <progress-bar class="{{ block.settings.color_scheme }}" data-quantity="{{ product.selected_or_first_available_variant.inventory_quantity }}" data-total="{{ block.settings.base_quantity }}"></progress-bar>
                                      {% endif %}
                                    </li>
                                  {%- elsif inventory_quantity <= 0 and product.selected_or_first_available_variant.inventory_policy == 'deny' -%}
                                    {% unless product.selected_or_first_available_variant.incoming and block.settings.show_transfer %}
                                      <li class="product__inventory-item product__inventory-item--sold" style="--out-of-stock-color: {{ block.settings.sold_out_color.red }}, {{ block.settings.sold_out_color.green }}, {{ block.settings.sold_out_color.blue }};">
                                        {% if block.settings.show_label %}
                                          <p class="product__inventory-label label--{{ settings.typography_preset }} {% if block.settings.label_size != 'default' %}{{ block.settings.label_size }}{% else %}{% if settings.typography_preset == 'custom' %}{% if settings.uppercase_body_s %} uppercase{% endif %}{% if settings.bolder_font_body_s %} bolder-font{% endif %}{% endif %}{% endif %} {% if settings.dim_text %}dim{% endif %}">{{ 'products.product.inventory.inventory_label' | t }}</p>
                                        {% endif %}
                                        <p class="product__inventory-status input--{{ settings.typography_preset }} {% if block.settings.text_size != 'default' %}{{ block.settings.text_size }}{% else %}{% if settings.typography_preset == 'custom' %}{% if settings.uppercase_body_s %} uppercase{% endif %}{% if settings.bolder_font_body_s %} bolder-font{% endif %}{% endif %}{% endif %}">{{ 'products.product.inventory.out_of_stock' | t: count: product.selected_or_first_available_variant.inventory_quantity }}</p>
                                        {% if block.settings.show_bar %}
                                          <progress-bar class="progress-bar"></progress-bar>
                                        {% endif %}
                                      </li>                        
                                    {% endunless %}
                                  {%- endif -%}
                                {%- if block.settings.show_transfer and product.selected_or_first_available_variant.incoming -%}
                                  {% if inventory_quantity <= 0 %}
                                    <li class="product__inventory-item">
                                      {% if block.settings.show_label %}
                                        <p class="product__inventory-label label--{{ settings.typography_preset }} {% if block.settings.label_size != 'default' %}{{ block.settings.label_size }}{% else %}{% if settings.typography_preset == 'custom' %}{% if settings.uppercase_body_s %} uppercase{% endif %}{% if settings.bolder_font_body_s %} bolder-font{% endif %}{% endif %}{% endif %} {% if settings.dim_text %}dim{% endif %}">{{ 'products.product.inventory.inventory_label' | t }}</p>
                                      {% endif %}
                                      <span class="icon-and-text">
                                        <span class="icon icon--inventory"></span>
                                        <span class="js-incoming-text input--{{ settings.typography_preset }} {% if block.settings.text_size != 'default' %}{{ block.settings.text_size }}{% else %}{% if settings.typography_preset == 'custom' %}{% if settings.uppercase_body_s %} uppercase{% endif %}{% if settings.bolder_font_body_s %} bolder-font{% endif %}{% endif %}{% endif %}" style="--in-stock-color: {{ block.settings.in_stock_color.red }}, {{ block.settings.in_stock_color.green }}, {{ block.settings.in_stock_color.blue }};">
                                          {%- liquid
                                            if product.selected_or_first_available_variant.next_incoming_date
                                              assign date = product.selected_or_first_available_variant.next_incoming_date | date: format: 'date'
                                              if product.selected_or_first_available_variant.inventory_quantity <= 0
                                                echo 'products.product.inventory.will_not_ship_until' | t: date: date
                                              else
                                                echo 'products.product.inventory.will_be_in_stock_after' | t: date: date
                                              endif
                                            else
                                              echo 'products.product.inventory.waiting_for_stock' | t
                                            endif
                                          -%}
                                        </span>
                                      </span>
                                    </li>
                                  {% endif %}
                                {%- endif -%}
                              </ul>
                            {% endif %}
                          {% when 'linked_products' %}
                            {%  render 'linked-products', product: product, block: block %}
                          {%- when 'custom_options_pills' -%}
                            {%- render 'custom-options-pills', block: block, weight_accordion_title: weight_accordion_title -%}
                          {%- when 'custom_options_measur' -%}
                              {%- render 'custom-options-measurement', block: block, weight_accordion_title: weight_accordion_title -%}
                          {%- when 'custom_options_checkbox' -%}
                              {%- render 'custom-options-checkbox', block: block, weight_accordion_title: weight_accordion_title -%}
                          {%- when 'custom_options_dropdown' -%}
                              {%- render 'custom-options-dropdown', block: block, weight_accordion_title: weight_accordion_title -%}
                          {%- when 'custom_options_s_line' -%}
                              {%- render 'custom-options-single-line', block: block, weight_accordion_title: weight_accordion_title -%}
                          {%- when 'custom_options_m_line' -%}
                              {%- render 'custom-options-multi-line', block: block, weight_accordion_title: weight_accordion_title -%}
                          {%- when 'custom_options_upload' -%}
                              {%- render 'custom-options-upload', block: block, weight_accordion_title: weight_accordion_title -%}
                          {%- when 'custom_options_date' -%}
                              {%- render 'custom-options-date', block: block, weight_accordion_title: weight_accordion_title -%}
                          {%- when 'custom_options_swatches' -%}
                              {%- render 'custom-options-swatches', block: block, weight_accordion_title: weight_accordion_title -%}
                          {%- when 'custom_options_color' -%}
                              {%- render 'custom-options-color', block: block, weight_accordion_title: weight_accordion_title -%}
                        {% endcase %}
                      {% endfor %}
                    </div>
                  </div>
                <div>
              {% endif %}
          </div>
        </div>
      </div>
    </div>
  </div>

  {% if section.settings.enable_image_zoom and section.settings.image_zoom_style != 'zoom-in' %}
    <product-modal id="ProductModal-{{ section.id }}" class="product-media-modal">
      {% style %}
        #ProductModal-{{ section.id }} {
          --desktop-grid-type: 1;
          --item-width: {{ section.settings.desktop_width }}%;
          --focal-point: {{ section.settings.zoom_focal_point }};
          --slider-gap: 8px;
          --overlay-opacity: {{ section.settings.zoom_overlay_opacity | divided_by: 100.0 }};
        }

        @media screen and (max-width: 920px) {
          #ProductModal-{{ section.id }} {
          --item-width: {{ section.settings.mobile_width }}%;
        }
      }
      {% endstyle %}

      {%- liquid 
        if settings.typography_preset == 'avant_garde'
            assign weight_close = settings.accent_icon_weight
            if settings.swap_fonts
                assign weight_close = settings.base_icon_weight
            endif
        else 
            assign weight_close =  settings.base_icon_weight
            if settings.swap_fonts
                assign weight_close = settings.accent_icon_weight
            endif
        endif
        if settings.typography_preset == 'custom'
          assign weight_close = settings.custom_icon_weight
        endif
      -%}
      
      <div class="product-media-modal__dialog {% if settings.enable_buttons_zoom %}buttons-zoom{% endif %}" role="dialog" aria-label="{{ 'products.modal.label' | t }}" aria-modal="true" tabindex="-1">
        <button id="ModalClose-{{ section.id }}" type="button" class="product-media-modal__toggle button-close" aria-label="{{ 'accessibility.close' | t }}">
          {% if weight == 'regular' %}
              {% render 'icon', icon: 'cross-regular' %}
          {% elsif weight == 'medium' %}
              {% render 'icon', icon: 'cross-medium' %}
          {% elsif weight == 'bold' %}
              {% render 'icon', icon: 'cross-bold' %}
          {% endif %}
        </button>

        <div class="product-media-modal__content gradient" role="document" aria-label="{{ 'products.modal.label' | t }}" tabindex="0">
          <div class="product-media-modal__wrapper product-media-modal__wrapper--{{ section.settings.image_zoom_style }} 
            {% if settings.enable_blending and section.settings.image_zoom_style == 'default' %} enable-blending {% endif %}  
            {% if section.settings.image_zoom_style == 'slideshow' %} slider-container-js{% endif %}"
          >
          {% assign variant_images = product.images | where: 'attached_to_variant?', true | map: 'src' %}
          {% assign variant_alt = product.selected_or_first_available_variant.image.alt | downcase  %}
          {% liquid 
            assign admin_order = false
            if section.settings.organize_images 
              assign admin_order = true
            endif
            if section.settings.first_image_instead_of_variant
              assign admin_order = true
            endif
          %}
          {% if section.settings.image_zoom_style == 'slideshow' %}
            <slider-component id="GalleryViewer-{{ section.id }}-zoom" class="slider">
              <div class="slider__viewport">
                <ul id="Slider-Gallery-{{ section.id }}-zoom" class="slider__grid slider__grid-mobile-1" role="list" 
                  data-count="1"
                  data-count-mobile="1">
          {% endif %}
          {% if admin_order == false %}
            {%- liquid
              if product.selected_or_first_available_variant.featured_media != null
                assign media = product.selected_or_first_available_variant.featured_media
                assign featured_media_alt = product.selected_or_first_available_variant.featured_media.alt | downcase
                if product.selected_or_first_available_variant.featured_media.alt contains '(' or product.selected_or_first_available_variant.featured_media.alt contains ')' 
                  assign featured_media_alt = product.selected_or_first_available_variant.featured_media.alt | downcase | split: '(' | last | split: ')' | first | prepend: '(' | append: ')'
                else
                  assign featured_media_alt = product.selected_or_first_available_variant.featured_media.alt | downcase
                endif
                if featured_media_alt contains '(' or featured_media_alt contains ')'
                  assign general_media = false 
                else
                  if section.settings.show_extra_images 
                    assign general_media = true
                  endif
                endif
                render 'product-media', media: media, loop: section.settings.enable_video_looping, controls: enable_video_controls, autoplay: section.settings.enable_autoplay, variant_image: section.settings.show_media_attached_to_variant, current_variant_alt: true, media_alt: featured_media_alt, hide_images: section.settings.show_media_attached_to_variant, image_ratio: section.settings.zoom_image_ratio, style: section.settings.image_zoom_style, product_media: product.media, first: true, general_media: general_media
              endif
            -%}
          {% endif %}
          {%- for media in product.media -%}
            {%- liquid
              if media.alt contains '(' or media.alt contains ')' 
                assign media_alt = media.alt | downcase | split: '(' | last | split: ')' | first | prepend: '(' | append: ')' 
              else 
                assign media_alt = media.alt | downcase
              endif
              
              if variant_images contains media.src
                assign variant_image = true
              else
                assign variant_image = false
              endif
              if variant_alt contains media_alt
                assign current_variant_alt = true
              else
                assign current_variant_alt = false
              endif
              assign admin_order = false
              if section.settings.organize_images 
                assign admin_order = true
              endif
              if section.settings.first_image_instead_of_variant
                assign admin_order = true
              endif
              if media.alt contains '(' and media.alt contains ')'
                assign general_media = false 
              else
                if section.settings.show_extra_images 
                  assign general_media = true
                endif
              endif
              if admin_order == false 
                unless media.id == product.selected_or_first_available_variant.featured_media.id
                  render 'product-media', media: media, loop: section.settings.enable_video_looping, controls: enable_video_controls, autoplay: section.settings.enable_autoplay, variant_image: variant_image, current_variant_alt: current_variant_alt, media_alt: media_alt, hide_images: section.settings.show_media_attached_to_variant, image_ratio: section.settings.zoom_image_ratio, style: section.settings.image_zoom_style, for_loop: forloop, product_media: product.media, general_media: general_media
                endunless
              elsif section.settings.organize_images or section.settings.first_image_instead_of_variant
                render 'product-media', media: media, loop: section.settings.enable_video_looping, controls: enable_video_controls, autoplay: section.settings.enable_autoplay, variant_image: variant_image, current_variant_alt: current_variant_alt, media_alt: media_alt, hide_images: section.settings.show_media_attached_to_variant, image_ratio: section.settings.zoom_image_ratio, style: section.settings.image_zoom_style, for_loop: forloop, product_media: product.media, general_media: general_media
              endif
            -%}
          {%- endfor -%}
          {% if section.settings.image_zoom_style == 'slideshow' %}
                </ul>
                <div class="zoom-slider-buttons-items {% unless section.settings.show_arrow_in_mobile %} hidden-on-mobile{% endunless %}
                  {% if product.media.size == 1 %}hidden-mobile{% endif %}
                  {% if product.media.size == 1 %}hidden-desktop{% endif %}">
                  <button type="button" class="slider-button slider-button--prev" name="previous" aria-label="{{ 'general.slider.previous_slide' | t }}">
                    <div class="slider-button--wrapper">
                    {% if weight_close == 'regular' %}
                      {% render 'icon-accordion', icon: 'arrow-left-regular' %}
                    {% elsif weight_close == 'medium' %}
                      {% render 'icon-accordion', icon: 'arrow-left-medium' %}
                    {% elsif weight_close == 'bold' %}
                      {% render 'icon-accordion', icon: 'arrow-left-bold' %}
                    {% endif %}
                    </div>
                  </button>
                  <button type="button" class="slider-button slider-button--next" name="next" aria-label="{{ 'general.slider.next_slide' | t }}">
                    <div class="slider-button--wrapper">
                    {% if weight_close == 'regular' %}
                      {% render 'icon-accordion', icon: 'arrow-right-regular' %}
                    {% elsif weight_close == 'medium' %}
                      {% render 'icon-accordion', icon: 'arrow-right-medium' %}
                    {% elsif weight_close == 'bold' %}
                      {% render 'icon-accordion', icon: 'arrow-right-bold' %}
                    {% endif %}
                  </div>
                  </button>
                </div>
              </div>
            </slider-component>
          {% endif %}
          </div>
        </div>
      </div>
    </product-modal>
  {% endif %}
  {% assign popups = section.blocks | where: "type", "popup" %}
  {%- for block in popups -%}
    <modal-window id="PopupModal-{{ block.id }}" class="product-popup-modal" {{ block.shopify_attributes }}>
