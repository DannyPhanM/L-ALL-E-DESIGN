                                        {% render 'icon', icon: 'dropdown-chevron-left-medium' %}
                                    {% elsif weight == 'bold' %}
                                        {% render 'icon', icon: 'dropdown-chevron-left-bold' %}
                                    {% endif %}
                                </span>
                            {% endif %}
                        </button>
                        <button class="slideshow__controls--next slideshow__controls-button slideshow__controls-button--classic {% if section.settings.fill_controls == false %}slideshow__controls-button--long{% endif %} {% if section.settings.controls == 'classic_arrows' and section.settings.fill_controls %}slideshow__controls-button--filled{% endif %}" aria-label="{{ 'general.slider.next_slide' | t }}">
                            {% if section.settings.fill_controls == false %}
                                <span class="slideshow__controls-icon slideshow__controls-icon--long">
                                    {% if weight == 'regular' %}
                                        {% render 'icon-accordion-2', icon: 'arrow-long-right-regular' %}
                                    {% elsif weight == 'medium' %}
                                        {% render 'icon-accordion-2', icon: 'arrow-long-right-medium' %}
                                    {% elsif weight == 'bold' %}
                                        {% render 'icon-accordion-2', icon: 'arrow-long-right-bold' %}
                                    {% endif %}
                                </span>
                            {% else %}
                                <span class="slideshow__controls-icon slideshow__controls-icon--short">
                                    {% if weight == 'regular' %}
                                        {% render 'icon', icon: 'dropdown-chevron-regular' %}
                                    {% elsif weight == 'medium' %}
                                        {% render 'icon', icon: 'dropdown-chevron-medium' %}
                                    {% elsif weight == 'bold' %}
                                        {% render 'icon', icon: 'dropdown-chevron-bold' %}
                                    {% endif %}
                                </span>
                            {% endif %}
                        </button>
                {% endif %}
            </div>
        </div>
    </slideshow-component>
</div>
{% endif %}

