{% if section.blocks.size > 0 %}
    {% liquid 
        if section.index == 1
            assign is_first_section = true
        else
            assign is_first_section = false
        endif
    %}
{{ 'slideshow-with-media.css' | asset_url | stylesheet_tag: preload: is_first_section }}

{% liquid
    assign rtl_languages = "ar,he,fa,ur,ps,ks" | split: "," 
    assign shop_locale_code = request.locale.iso_code
    assign is_rtl = false

    if rtl_languages contains shop_locale_code
        assign is_rtl = true
    endif
        
    if settings.typography_preset == 'avant_garde'
            assign weight = settings.accent_icon_weight
            if settings.swap_fonts
                    assign weight = settings.base_icon_weight
            endif
    else 
            assign weight =  settings.base_icon_weight
            if settings.swap_fonts
                    assign weight = settings.accent_icon_weight
            endif
    endif
    if settings.typography_preset == 'custom'
        assign weight = settings.custom_icon_weight
    endif
    assign animationDuration = 0
    if settings.animation_duration == '0.3'
        assign animationDuration = 'basic'
    elsif settings.animation_duration == '0.5'
        assign animationDuration = 'sophisticated'
    endif

    assign lazy_loading_images = false
    if section.index > 2 and settings.images_loading != 'disable'
        assign lazy_loading_images = true
    endif
%}

{% style %}
    #shopify-section-{{ section.id }} {
        --padding-top: {{ section.settings.desktop_padding_top }}px;
        --padding-bottom: {{ section.settings.desktop_padding_bottom }}px;
        --section-height: {{ section.settings.desktop_height }}vh;
        position: relative;
        {% if section.settings.color_type == 'invert' %}
            --layout-text-color:
            {{ settings.layout_background_color.red }},
            {{ settings.layout_background_color.green }},
            {{ settings.layout_background_color.blue }};
            --layout-background-color:
            {{ settings.layout_text_color.red }},
            {{ settings.layout_text_color.green }}, 
            {{ settings.layout_text_color.blue }};
            {% endif %}
        {% if section.settings.color_type == 'custom' %}
            --layout-text-color:
            {{ section.settings.text_color.red }},
            {{ section.settings.text_color.green }}, 
            {{ section.settings.text_color.blue }};
            --layout-background-color:
            {{ section.settings.background_color.red }},
            {{ section.settings.background_color.green }}, 
            {{ section.settings.background_color.blue }};
        {% endif %}
    }
    {% if section.settings.section_width == 'overlap' and settings.allow_transparent_navigation %}
      body:has(main #shopify-section-{{ section.id }}:first-child .overlapping-section) .scroll-color.section-color,
      body:has(main #shopify-section-{{ section.id }}:first-child .overlapping-section) .transparent-header.transparent,
      body:has(main #shopify-section-{{ section.id }}:first-child .overlapping-section) .shopify-section-group-more-header-sections.transparent  {
        {% if section.settings.color_type == 'invert' %}
            --layout-text-color:
            {{ settings.layout_background_color.red }},
            {{ settings.layout_background_color.green }},
            {{ settings.layout_background_color.blue }};
            --layout-background-color:
            {{ settings.layout_text_color.red }},
            {{ settings.layout_text_color.green }}, 
            {{ settings.layout_text_color.blue }};
            --accent-button-color:  
            {{ settings.layout_background_color.red }},
            {{ settings.layout_background_color.green }},
            {{ settings.layout_background_color.blue }};
            --base-button-color: 
            {{ settings.layout_text_color.red }},
            {{ settings.layout_text_color.green }}, 
            {{ settings.layout_text_color.blue }};
        {% endif %}
        {% if section.settings.color_type == 'custom' %}
            --layout-text-color:
            {{ section.settings.text_color.red }},
            {{ section.settings.text_color.green }}, 
            {{ section.settings.text_color.blue }};
            --layout-background-color:
            {{ section.settings.background_color.red }},
            {{ section.settings.background_color.green }}, 
            {{ section.settings.background_color.blue }};
            --accent-button-color: 
            {{ section.settings.text_color.red }},
            {{ section.settings.text_color.green }}, 
            {{ section.settings.text_color.blue }};
            --base-button-color: 
            {{ section.settings.background_color.red }},
            {{ section.settings.background_color.green }}, 
            {{ section.settings.background_color.blue }};
        {% endif %} 
    }
    body:has(main #shopify-section-{{ section.id }}:first-child .overlapping-section) .main-sidebar.overlap-slide-out-menu-opened .scroll-color.section-color {
        --layout-background-color: 
        {{ settings.layout_background_color.red }},
        {{ settings.layout_background_color.green }},
        {{ settings.layout_background_color.blue }};
        --layout-text-color: 
        {{ settings.layout_text_color.red }},
        {{ settings.layout_text_color.green }}, 
        {{ settings.layout_text_color.blue }};
        --base-button-color: 
        {{ settings.base_button_color.red }},
        {{ settings.base_button_color.green }},
        {{ settings.base_button_color.blue }};
        --accent-button-color: 
        {{ settings.accent_button_color.red }},
        {{ settings.accent_button_color.green }},
        {{ settings.accent_button_color.blue }};
    }
    @media (hover: hover) {
        body:has(main #shopify-section-{{ section.id }}:first-child .overlapping-section) .main-sidebar.transparent-sidebar:has(.menu__dropdown-container--overlap-wrapper):hover  .scroll-color.section-color {
          --layout-background-color: 
          {{ settings.layout_background_color.red }},
          {{ settings.layout_background_color.green }},
          {{ settings.layout_background_color.blue }};
          --layout-text-color: 
          {{ settings.layout_text_color.red }},
          {{ settings.layout_text_color.green }}, 
          {{ settings.layout_text_color.blue }};
          --base-button-color: 
          {{ settings.base_button_color.red }},
          {{ settings.base_button_color.green }},
          {{ settings.base_button_color.blue }};
          --accent-button-color: 
          {{ settings.accent_button_color.red }},
          {{ settings.accent_button_color.green }},
          {{ settings.accent_button_color.blue }};
        }
      }
    body:has(main #shopify-section-{{ section.id }}:first-child .overlapping-section) .shopify-section-group-more-header-sections.transparent {
        background-color: transparent;
    }
    {% endif %}
    @media screen and (min-width: 921px) {
        {% if section.settings.section_width == 'overlap' and settings.allow_transparent_navigation %}
            #shopify-section-{{ section.id }}:first-child {
                margin-top: -100px;
            }
            @supports selector(:has(+ *)) {
                #shopify-section-{{ section.id }}:first-child {
                    margin-top: 0px;
                }
            }
            #shopify-section-{{ section.id }}:first-child::after {
                content: "";
                position: absolute;
                width: 100vw;
                height: 100%;
                top:0;
                z-index: 0;
                background-color: rgb(var(--layout-background-color));
                /* will-change: transform; */
            }
        {% endif %}
        #shopify-section-{{ section.id }}:first-child .slideshow__image,
        #shopify-section-{{ section.id }}:first-child::after {
            {% if section.settings.section_width == 'overlap' and settings.allow_transparent_navigation %}
                width: 100vw !important;
            {% endif %}
        
            {% if is_rtl %}
                {% if settings.navigation_layout == 'sidebar_based' and settings.white_space == 'compact' and section.settings.section_width == 'overlap' and settings.allow_transparent_navigation %}
                    {% if settings.sidebar_template == 'main_xs' or settings.sidebar_template == 'main_xs_with_secondary' %}
                        transform: translate(max(12.5%, 200px));
                    {% elsif settings.sidebar_template == 'main_s' or settings.sidebar_template == 'main_s_with_secondary' %}
                        transform: translate(max(18.75%, 200px));
                    {% elsif settings.sidebar_template == 'main_m' or settings.sidebar_template == 'main_m_with_secondary' %}
                        transform: translate(max(25%, 200px));
                    {% elsif settings.sidebar_template == 'main_l' %}
                        transform: translate(max(50%, 200px));
                    {% endif %}
                {% endif %}
                {% if settings.navigation_layout == 'sidebar_based' and settings.white_space == 'spacious' and section.settings.section_width == 'overlap' and settings.allow_transparent_navigation %}
                    {% if settings.sidebar_template == 'main_xs' or settings.sidebar_template == 'main_xs_with_secondary' %}
                        transform: translate(max(12.5%, 280px));
                    {% elsif settings.sidebar_template == 'main_s' or settings.sidebar_template == 'main_s_with_secondary' %}
                        transform: translate(max(18.75%, 280px));
                    {% elsif settings.sidebar_template == 'main_m' or settings.sidebar_template == 'main_m_with_secondary' %}
                        transform: translate(max(25%, 280px));
                    {% elsif settings.sidebar_template == 'main_l' %}
                        transform: translate(max(50%, 280px));
                    {% endif %}
                {% endif %}
            {% else %}
                {% if settings.navigation_layout == 'sidebar_based' and settings.white_space == 'compact' and section.settings.section_width == 'overlap' and settings.allow_transparent_navigation %}
                    {% if settings.sidebar_template == 'main_xs' or settings.sidebar_template == 'main_xs_with_secondary' %}
                        transform: translate(min(-12.5%, -200px));
                    {% elsif settings.sidebar_template == 'main_s' or settings.sidebar_template == 'main_s_with_secondary' %}
                        transform: translate(min(-18.75%, -200px));
                    {% elsif settings.sidebar_template == 'main_m' or settings.sidebar_template == 'main_m_with_secondary' %}
                        transform: translate(min(-25%, -200px));
                    {% elsif settings.sidebar_template == 'main_l' %}
                        transform: translate(min(-50%, -200px));
                    {% endif %}
                {% endif %}
                {% if settings.navigation_layout == 'sidebar_based' and settings.white_space == 'spacious' and section.settings.section_width == 'overlap' and settings.allow_transparent_navigation %}
                    {% if settings.sidebar_template == 'main_xs' or settings.sidebar_template == 'main_xs_with_secondary' %}
                        transform: translate(min(-12.5%, -280px));
                    {% elsif settings.sidebar_template == 'main_s' or settings.sidebar_template == 'main_s_with_secondary' %}
                        transform: translate(min(-18.75%, -280px));
                    {% elsif settings.sidebar_template == 'main_m' or settings.sidebar_template == 'main_m_with_secondary' %}
                        transform: translate(min(-25%, -280px));
                    {% elsif settings.sidebar_template == 'main_l' %}
                        transform: translate(min(-50%, -280px));
                    {% endif %}
                {% endif %}
            {% endif %}
        }
        
    }
    #shopify-section-{{ section.id }}:first-child .overlapping-section {
        padding-bottom: 0!important;
        padding-top: 0!important;
    }
    @media screen and (max-width: 920px) {
        #shopify-section-{{ section.id }} {
            --section-height: {{ section.settings.mobile_height }}vh;
        }
        #shopify-section-{{ section.id }}:first-child .slideshow__image:first-of-type {
            transform: unset;
        }
        #shopify-section-{{ section.id }}:first-child,
        #shopify-section-{{ section.id }}:first-child .overlapping-section .overlapping-content-js {
            margin-top: 0 !important;
        }
    }
  {% endstyle %}

<div class="ordinal-section ordinal-section--{{ settings.white_space }} first-loading  
    {%- if section.settings.section_width == 'overlap' %} no-padding {% endif -%}  
    {%- if settings.allow_transparent_navigation and section.settings.section_width == 'overlap' %} overlapping-section {% if is_first_section %} overlapping-section--first {% endif %} {% endif -%}  
    {%- if section.settings.desktop_height == 100 %} full-height--desktop {% endif -%} 
    {%- if section.settings.mobile_height == 100 %} full-height--mobile {% endif -%} 
    {%- if section.settings.section_width == 'wide' %} section-padding--{{ settings.white_space }} {% endif -%} 
    {%- if section.settings.color_type != 'default' %} no-default-color {% endif -%}"
>
    <slideshow-component>
        <div class="slideshow slideshow--{{ settings.white_space }} {% if settings.allow_transparent_navigation and section.settings.section_width == 'overlap' %}slideshow--overlapping{% endif %}">
            <div class="slideshow__slider animation-fade {{ animationDuration }}" {% if section.settings.enable_autoplay %}data-autoplay="{{ section.settings.autoplay_speed | times: 1000.00 }}"{% endif %}>
                {% for block in section.blocks %}
                    {%- case block.type -%}
                        {% when 'image_slide' %}
                            {% if block.settings.block_link != blank %}<a href="{{ block.settings.block_link }}" {% if block.settings.block_target_blank %}target="_blank"{% endif %} class="slideshow__slide {% if forloop.first %}current{% endif %} loaded slideshow__slide-{{ block.id }} {{ section.settings.section_width }}" data-position="{{ forloop.index }}" id="{{ block.id }}" {{ block.shopify_attributes }}>
                            {% else %}<div class="slideshow__slide {% if forloop.first %}current before-load{% endif %} loaded slideshow__slide-{{ block.id }} {{ section.settings.section_width }}" data-position="{{ forloop.index }}" id="{{ block.id }}" {{ block.shopify_attributes }}>{% endif %}
                                {% style %}
                                    .slideshow__slide-{{ block.id }} {
                                        --vertical-position: {{ block.settings.vertical_content_position }};
                                        --horizontal-position: {{ block.settings.horizontal_content_position }};
                                        {% if block.settings.overlay_gradient == blank %}
                                            --overlay-background: {{ block.settings.overlay_color }};
                                        {% else %}
                                            --overlay-background: {{ block.settings.overlay_gradient }};
                                        {% endif %}
                                        --overlay-opacity: {{ block.settings.overlay_opacity | divided_by: 100.0 }};
                                        --block-index: {{ forloop.index0 }};
                                    }
                                {% endstyle %}
                                <div class="slideshow__content-container {% if block.settings.center_text %}slideshow__content-container--center{% endif %}">
                                    <div class="slideshow__content-wrapper {% if block.settings.fill_content %}slideshow__content-wrapper--filled{% endif %} overlapping-content-js {% if section.settings.controls == 'classic_arrows' and section.blocks.size > 1 %}slideshow__content-wrapper--classic-arrows{% endif %} {% if section.blocks.size > 1 %}slideshow__content-wrapper--{{ block.settings.vertical_content_position }}{% endif %} overlapping-content-js slideshow__content-wrapper--{{ block.id }} {% if block.settings.center_text %}center{% endif %}">
                                        {% style %}
                                        .slideshow__content-wrapper--{{ block.id }} {
                                            --context-image-width: {{ block.settings.context_image_width }}px;
                                                {% if block.settings.context_image_radius == 'default' %}
                                                --context-image-radius: {{ settings.images_and_sections_radius }};
                                                {% else %}
                                                --context-image-radius: {{ block.settings.context_image_radius }};
                                                {% endif %}
                                        }
                                        .slideshow__content-wrapper--{{ block.id }} .context-image.ratio-original .lazy-image {
                                            aspect-ratio: var(--asp-rat);
                                        }
                                        {% endstyle %}
                                        {% if block.settings.context_image != blank or block.settings.subheading != blank or block.settings.heading != blank or block.settings.text != blank or block.settings.button_label_1 != blank or block.settings.button_label_2 != blank or block.settings.countdown_enable %}
                                        <div class="slideshow__content slideshow__content-js block--{{ block.id }} {% if block.settings.narrow_content %}slideshow__content--narrow{% endif %} {% if block.settings.center_text %}slideshow__content--center{% endif %} {{ block.settings.fill_content }} {% if block.settings.fill_content %}slideshow__content--filled{% endif %}">
                                            {% if block.settings.context_image != blank %}
                                                {%- if block.settings.context_image_link != blank -%}{% if block.settings.block_link != blank %}<object>{% endif %}<a href="{{ block.settings.context_image_link }}"  {% if block.settings.context_image_target_blank %}target="_blank"{% endif %} class="context-image ratio-{{ block.settings.image_ratio }}" {% if settings.disable_right_click %}oncontextmenu="return false"{% endif %}>
                                                {%- else -%}<div class="context-image ratio-{{ block.settings.image_ratio }}" {% if settings.disable_right_click %}oncontextmenu="return false"{% endif %}>{%- endif -%}
                                                    {% comment %} theme-check-disable ImgLazyLoading {% endcomment %}
                                                {% if lazy_loading_images %}
                                                    <figure class="lazy-image lazy-image--{{ settings.images_loading }}" style="--asp-rat: {{ block.settings.context_image.aspect_ratio }};">
                                                {% endif %}
                                                    <img
                                                        srcset="{% if block.settings.context_image.width >= 350 %}{{ block.settings.context_image | image_url: width: 350 }} 350w,{% endif %}
                                                            {% if block.settings.context_image.width >= 750 %}{{ block.settings.context_image | image_url: width: 750 }} 750w,{% endif %}
                                                            {% if block.settings.context_image.width >= 1100 %}{{ block.settings.context_image | image_url: width: 1100 }} 1100w,{% endif %}
                                                            {% if block.settings.context_image.width >= 1500 %}{{ block.settings.context_image | image_url: width: 1500 }} 1500w,{% endif %}
                                                            {% if block.settings.context_image.width >= 2200 %}{{ block.settings.context_image | image_url: width: 2200 }} 2200w,{% endif %}
                                                            {% if block.settings.context_image.width >= 3000 %}{{ block.settings.context_image | image_url: width: 3000 }} 3000w,{% endif %}
                                                            {{ block.settings.context_image | image_url }} {{ block.settings.context_image.width }}w"
                                                        sizes="(min-width: 750px) calc(100vw - 10rem), (min-width: 750px) calc(100vw - 10rem), 100vw"
                                                        src="{{ block.settings.context_image | image_url: width: 1100 }}"
                                                        {% if lazy_loading_images %}
                                                            loading="lazy"
                                                            onload="this.parentNode.classList.add('lazyloaded');"
                                                        {% else %}
                                                            {% if section.index > 2 or forloop.index > 1 %}loading="lazy"{% else %}loading="eager"{% endif %}
                                                        {% endif %}
                                                        draggable="false"
                                                        width="{{ block.settings.context_image_width }}"
                                                        height="{{ block.settings.context_image.height }}"
                                                        alt="{{ block.settings.context_image.alt | escape }}"
                                                        class="context__image-item"
                                                        style="object-position: {{ block.settings.context_image.presentation.focal_point }};">
                                                    {% if lazy_loading_images %}
                                                        {% if settings.images_loading != 'fade_scale' %}
                                                        <div class="lazy-image__preloader lazy-image__preloader-{{ settings.images_loading }}" aria-hidden="true">
                                                            <img 
                                                            alt="{{ block.settings.context_image.alt | escape }}" 
                                                            src="{{ block.settings.context_image | image_url: width: 10 }}"
                                                            draggable="false"
                                                            width="10" 
                                                            height="10">
                                                        </div>
                                                        {% endif %}
                                                    </figure>
                                                    {% endif %}
                                                        {% comment %} theme-check-enable ImgLazyLoading {% endcomment %}
                                                {%- if block.settings.context_image_link != blank -%}</a>{% if block.settings.block_link != blank %}</object>{% endif %}
                                                {%- else -%}</div>{%- endif -%}
                                            {% endif %}
                                            {% if block.settings.subheading != blank %}
                                                <{{ block.settings.subheading_tag }} class="content-subheading content-subheading--{{ settings.typography_preset }} {% if block.settings.subheading_size != 'default' %}{{ block.settings.subheading_size }}{% else %}{% if settings.typography_preset == 'custom' %}{% if settings.uppercase_label %} uppercase{% endif %}{% if settings.bolder_font_label %} bolder-font{% endif %}{% endif %}{% endif %} {% if block.settings.dim_text %}dim{% endif %}">{{ block.settings.subheading }}</{{ block.settings.subheading_tag }}>
                                            {% endif %}
                                            {% if block.settings.heading != blank %}   
                                                {% liquid
                                                    assign heading_text = block.settings.heading
                                                    assign final_text = ''
                                                    assign split_text_curly = heading_text | split: '{'
                                                    for part in split_text_curly
                                                        if part contains '}'
                                                            assign split_curly = part | split: '}'
                                                            assign styled_text = split_curly[0]
                                                            assign after_curly = split_curly[1]
                                                            assign final_text = final_text | append: '<span class="styled">' | append: styled_text | append: '</span>' | append: after_curly
                                                        else
                                                            assign final_text = final_text | append: part
                                                        endif
                                                    endfor
                                                  %}
                                                <{{ block.settings.heading_tag }} class="content-heading custom-heading {% if section.settings.color_type == 'default' and settings.accent_color_for_headings %}content-heading--accent{% endif %} content-heading--{{ settings.typography_preset }} {% if block.settings.heading_size != 'default' %}{{ block.settings.heading_size }}{% else %}{% if settings.typography_preset == 'custom' %}{% if settings.uppercase_heading_xs %} uppercase{% endif %}{% if settings.bolder_font_heading_xs %} bolder-font{% endif %}{% endif %}{% endif %}">{{ final_text }}</{{ block.settings.heading_tag }}>
                                            {% endif %}
                                            {% if block.settings.text != blank %}  
                                                <div class="content-text content-text--{{ settings.typography_preset }} {% if block.settings.main_text_size != 'default' %}{{ block.settings.main_text_size }}{% else %}{% if settings.typography_preset == 'custom' %}{% if settings.uppercase_body_m %} uppercase{% endif %}{% if settings.bolder_font_body_m %} bolder-font{% endif %}{% endif %}{% endif %} {% if block.settings.dim_text %}dim{% endif %} richtext {% if settings.enable_moving_underline %}richtext-link--underline{% endif %}">
                                                {% if block.settings.block_link != blank %}<object>{% endif %}{{ block.settings.text }}{% if block.settings.block_link != blank %}</object>{% endif %}
                                                </div>
                                            {% endif %}

                                            {% if block.settings.countdown_enable %}
                                                {%- render 'countdown-timer', block: block, section_id: section.id, section_blocks_count: section.blocks.size -%}
                                            {% endif %}  

                                            {% if block.settings.button_label_1 != blank or block.settings.button_label_2 != blank %}
                                                <div class="slideshow__buttons">
                                                    {% if block.settings.button_label_1 != blank %}
                                                        <div class="slideshow__button {% if settings.enable_buttons_zoom %}buttons-zoom{% endif %}  {% unless block.settings.button_link_1 != blank %}disabled{% endunless %}">
                                                        {%- if block.settings.button_link_1 != blank -%}{% if block.settings.block_link != blank %}<object>{% endif %}<a tabindex="-1" href="{{ block.settings.button_link_1 }}" {% if block.settings.target_blank_1 %}target="_blank"{% endif %} class="{{ block.settings.button_style_1 }}-button button--{{ settings.typography_preset }} {% if settings.typography_preset == 'custom' %}{% if settings.regular_button_size == 'default' %}button-font--default{% else %}{{ settings.regular_button_size }}{% endif %}{% endif %} {% if block.settings.button_style_1 == 'link' and settings.enable_moving_underline %}link-button--underline{% endif %}">{%- else -%}
                                                        <span disabled class="{{ block.settings.button_style_1 }}-button button--{{ settings.typography_preset }} {% if settings.typography_preset == 'custom' %}{% if settings.regular_button_size == 'default' %}button-font--default{% else %}{{ settings.regular_button_size }}{% endif %}{% endif %} {% if block.settings.button_style_1 == 'link' and settings.enable_moving_underline %}link-button--underline{% endif %}">{%- endif -%}
                                                        {{ block.settings.button_label_1 }}
                                                        {%- unless block.settings.button_link_1 != blank -%}</span>{%- endunless -%}{%- if block.settings.button_link_1 != blank -%}</a>{% if block.settings.block_link != blank %}</object>{% endif %}{%- endif -%}
                                                        </div>
                                                    {% endif %}
                                                    {% if block.settings.button_label_2 != blank %}
                                                        <div class="slideshow__button {% if settings.enable_buttons_zoom %}buttons-zoom{% endif %}  {% unless block.settings.button_link_2 != blank %}disabled{% endunless %}">
                                                        {%- if block.settings.button_link_2 != blank -%}{% if block.settings.block_link != blank %}<object>{% endif %}<a tabindex="-1" href="{{ block.settings.button_link_2 }}" {% if block.settings.target_blank_2 %}target="_blank"{% endif %} class="{{ block.settings.button_style_2 }}-button button--{{ settings.typography_preset }} {% if settings.typography_preset == 'custom' %}{% if settings.regular_button_size == 'default' %}button-font--default{% else %}{{ settings.regular_button_size }}{% endif %}{% endif %} {% if block.settings.button_style_2 == 'link' and settings.enable_moving_underline %}link-button--underline{% endif %}">{%- else -%}
                                                        <span disabled class="{{ block.settings.button_style_2 }}-button button--{{ settings.typography_preset }} {% if settings.typography_preset == 'custom' %}{% if settings.regular_button_size == 'default' %}button-font--default{% else %}{{ settings.regular_button_size }}{% endif %}{% endif %} {% if block.settings.button_style_2 == 'link' and settings.enable_moving_underline %}link-button--underline{% endif %}">{%- endif -%}
                                                        {{ block.settings.button_label_2 }}
                                                        {%- unless block.settings.button_link_2 != blank -%}</span>{%- endunless -%}{%- if block.settings.button_link_2 != blank -%}</a>{% if block.settings.block_link != blank %}</object>{% endif %}{%- endif -%}
                                                        </div>
                                                    {% endif %}
                                                </div>
                                            {% endif %}
                                        </div>
                                        {% endif %}
                                    </div> 
                                </div>
                                <div class="slideshow__image overlapping-container-js" {% if settings.disable_right_click %}oncontextmenu="return false"{% endif %}>
                                    <div class="slideshow__image- slideshow__media" {% if settings.disable_right_click %}oncontextmenu="return false"{% endif %}>
                                        {% comment %} theme-check-disable ImgLazyLoading {% endcomment %}
