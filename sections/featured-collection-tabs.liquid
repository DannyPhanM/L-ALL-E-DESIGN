{% if section.blocks.size > 0 %}
{{ 'component-price.css' | asset_url | stylesheet_tag }}
{{ 'section-slider.css' | asset_url | stylesheet_tag }}
{{ 'component-product-card.css' | asset_url | stylesheet_tag }}
{{ 'component-slider.css' | asset_url | stylesheet_tag }}
{{ 'section-tabs.css' | asset_url | stylesheet_tag }}
{{ 'section-featured-collection.css' | asset_url | stylesheet_tag }}
{{ 'component-tab.css' | asset_url | stylesheet_tag }}
{{ 'component-product-features.css' | asset_url | stylesheet_tag }}
{{ 'section-multicolumn.css' | asset_url | stylesheet_tag }}

{% style %}
    #shopify-section-{{ section.id }} {
        --padding-top: {{ section.settings.desktop_padding_top }}px;
        --padding-bottom: {{ section.settings.desktop_padding_bottom }}px;
        --media-focal-point: {{ section.settings.focal_point }};
        --focal-point: {{ section.settings.card_focal_point }};
        --tabs-alignment: {% if section.settings.center_text %}center{% else %}flex-start{% endif %};
        {% if section.settings.image_radius == 'default' %}
            --image-radius: {{ settings.images_and_sections_radius }};
        {% else %}
            --image-radius: {{ section.settings.image_radius }};
        {% endif %}
        {% if section.settings.color_type == 'invert' %}
            --layout-text-color:
            {{ settings.layout_background_color.red }},
            {{ settings.layout_background_color.green }},
            {{ settings.layout_background_color.blue }};
            --layout-background-color:
            {{ settings.layout_text_color.red }},
            {{ settings.layout_text_color.green }}, 
            {{ settings.layout_text_color.blue }};
            --rating-color: 
            {{ settings.layout_background_color.red }},
            {{ settings.layout_background_color.green }},
            {{ settings.layout_background_color.blue }};
        {% endif %}
        {% if section.settings.color_type == 'custom' %}
            --layout-text-color:
            {{ section.settings.text_color.red }},
            {{ section.settings.text_color.green }}, 
            {{ section.settings.text_color.blue }};
            --layout-background-color:
            {{ section.settings.background_color.red }},
            {{ section.settings.background_color.green }}, 
            {{ section.settings.background_color.blue }};
            --rating-color: 
            {{ section.settings.text_color.red }},
            {{ section.settings.text_color.green }}, 
            {{ section.settings.text_color.blue }};
        {% endif %}  
        background-color: rgb(var(--layout-background-color));
        color: rgb(var(--layout-text-color));
    }
    @media screen and (max-width: 920px) {
        #shopify-section-{{ section.id }} {
            --padding-top: {{ section.settings.mobile_padding_top }}px;
            --padding-bottom: {{ section.settings.mobile_padding_bottom }}px;
        }
    }
    .quick-view-load #shopify-section-{{ section.id }} .component-tabs__content.active {
        position: static;
        /* will-change: auto; */
    }
{% endstyle %}

{% liquid
    assign settings_page_width = settings.max_page_width | append: 'px'
    if settings.max_page_width == 'full_width'
        assign settings_page_width = '100vw'
    endif
    assign limit_placeholder = section.settings.items_per_batch 
%}

{%- if section.settings.infinite_scroll == 'button' or section.settings.infinite_scroll == 'auto' -%}
    <link rel="stylesheet" href="{{ 'component-loading.css' | asset_url }}" media="print" onload="this.media='all'">
{%- endif -%}

<div class="featured-collection-with-product-grid multicolumn product-labels section-featured-collection section-featured-collection--{{ settings.white_space }} section-tabs tabs-container-js section-tabs--{{ settings.white_space }} ordinal-section ordinal-section--{{ settings.white_space }} section-padding--{{ settings.white_space }} {% if settings.show_lines %}ordinal-section--lines lines product-labels--lines{% endif %} {% if settings.max_page_width == '1400' or settings.max_page_width == '1200' %}page-width {% endif %} page-width--{{ settings.max_page_width }} color-{{ section.settings.color_type }} {% if section.settings.color_type != 'default' %}no-default-color{% endif %} product-labels--{{ settings.white_space }} product-labels--grid">
    <div class="section-tabs__header">
        {% if section.settings.subheading != blank %}
            <{{ section.settings.subheading_tag }} class="content-subheading content-subheading--{{ settings.typography_preset }} {% if section.settings.subheading_size != 'default' %}{{ section.settings.subheading_size }}{% else %}{% if settings.typography_preset == 'custom' %}{% if settings.uppercase_label %} uppercase{% endif %}{% if settings.bolder_font_label %} bolder-font{% endif %}{% endif %}{% endif %} {% if settings.dim_text %}dim{% endif %}">{{ section.settings.subheading }}</{{ section.settings.subheading_tag }}>
        {% endif %}
        {% if section.settings.heading != blank %}
          {% liquid
            assign heading_text = section.settings.heading
            assign final_text = ''
            assign split_text_curly = heading_text | split: '{'
            for part in split_text_curly
              if part contains '}'
                  assign split_curly = part | split: '}'
                  assign styled_text = split_curly[0]
                  assign after_curly = split_curly[1]
                  assign final_text = final_text | append: '<span class="styled">' | append: styled_text | append: '</span>' | append: after_curly
              else
                  assign final_text = final_text | append: part
              endif
            endfor
          %}
            <{{ section.settings.heading_tag }} class="content-heading custom-heading {% if section.settings.color_type == 'default' and settings.accent_color_for_headings %}content-heading--accent{% endif %} content-heading--{{ settings.typography_preset }} {% if section.settings.heading_size != 'default' %}{{ section.settings.heading_size }}{% else %}{% if settings.typography_preset == 'custom' %}{% if settings.uppercase_heading_xs %} uppercase{% endif %}{% if settings.bolder_font_heading_xs %} bolder-font{% endif %}{% endif %}{% endif %}">{{ final_text }}</{{ section.settings.heading_tag }}>
        {% endif %}
        <div class="component-tabs">
            <component-tabs class="component-tabs__wrapper {% if section.settings.center_text %}center{% endif %}">
                {%- for block in section.blocks -%}
                    <button id="{{ block.id }}" class="component-tabs__tab {% if forloop.first %}active{% endif %} component-tabs__tab--{{ settings.typography_preset }} {% if section.settings.tab_heading_size != 'default' %}{{ section.settings.tab_heading_size }}{% else %}{% if settings.typography_preset == 'custom' %}{% if settings.uppercase_body_l %} uppercase{% endif %}{% if settings.bolder_font_body_l %} bolder-font{% endif %}{% endif %}{% endif %}  tab-js tab-js-block-id--{{ block.id }}" {{ block.shopify_attributes }}>
                        {% if block.settings.custom_tab_heading != blank %}{{ block.settings.custom_tab_heading }}{% elsif block.settings.collection != blank and block.settings.custom_tab_heading == blank  %}{{ block.settings.collection.title | escape }}{% else %}{{ 'sections.featured_collection_tabs.tab_placeholder' | t }}{% endif %}
                    </button>
                {%- endfor -%}
            </component-tabs>
        </div>
    </div>
    <div class="section-tabs__content-wrapper">
        {%- for block in section.blocks -%}
            {% liquid
                assign mobile_layout = section.settings.mobile_layout | times: 1
                if block.settings.collection != blank
                    if block.settings.collection.products.size == 1
                        assign mobile_layout = 1
                    endif
                    if block.settings.collection.all_products_count < mobile_layout
                        assign mobile_layout = block.settings.collection.all_products_count
                    endif
                    if section.settings.max_columns == '2'
                        if block.settings.collection.products.size > 1
                            assign columns = section.settings.max_columns
                        else 
                            assign columns = '1'
                        endif
                    elsif section.settings.max_columns == '3'
                        if block.settings.collection.products.size > 2
                            assign columns = section.settings.max_columns
                        elsif block.settings.collection.products.size == 2 
                            assign columns = '2'
                        else 
                            assign columns = '1'
                        endif
                    elsif section.settings.max_columns == '4'
                        if block.settings.collection.products.size > 3
                            assign columns = section.settings.max_columns
                        elsif block.settings.collection.products.size == 3
                            assign columns = '3'
                        elsif block.settings.collection.products.size == 2 
                            assign columns = '2'
                        else 
                            assign columns = '1'
                        endif
                    endif
                else
                    assign columns = section.settings.max_columns
                endif
            %}
            <div id="content-{{ block.id }}" class="component-tabs__content tab-content-js {% if forloop.first %}active{% endif %} {% if settings.enable_buttons_zoom %}buttons-zoom{% endif %}" {{ block.shopify_attributes }}>
                {%- unless section.settings.media_style == 'hide' -%}
                    <div class="section-featured-collection__media lazy-image-container ratio-{{ section.settings.image_ratio }}{% if section.settings.media_style == 'show_on_desktop' %} show-on-desktop{% endif %}" {% if settings.disable_right_click %}oncontextmenu="return false"{% endif %}>
                        {% liquid 
                            assign lazy_load = false
                            if forloop.index > 1 or section.index > 2
                            assign lazy_load = true
                            endif
                        %}
                        {% comment %}theme-check-disable ImgLazyLoading{% endcomment %}
                        {%- if block.settings.video_load != blank -%}
                          {% if settings.images_loading == 'disable' %}
                            {{ block.settings.video_load | video_tag: image_size: "500x", autoplay: false, preload: 'none', loop: true, muted: true, controls: false, alt: block.settings.video_load.alt }}
                          {% else %}
                              <figure class="lazy-video lazy-image--{{ settings.images_loading }}" style="--desktop-asp-rat: {{ block.settings.video_load.aspect_ratio }}">
                                  {{ block.settings.video_load | video_tag: image_size: "500x", autoplay: false, preload: 'none', loop: true, muted: true, controls: false, alt: block.settings.video_load.alt, oncanplay: 'this.parentNode.classList.add("lazyloaded");' }}
                                  {% if settings.images_loading != 'fade_scale' %}
                                  <div class="lazy-image__preloader lazy-image__preloader--full lazy-image__preloader-{{ settings.images_loading }}" aria-hidden="true" >
                                      <img 
                                          alt="{{ block.settings.video_load.alt | escape }}" 
                                          src="{{ block.settings.video_load.preview_image | image_url: width: 10 }}"
                                          draggable="false"
                                          width="10" 
                                          height="10">
                                  </div>
                                  {% endif %}
                              </figure>
                          {% endif %}
                        {%- elsif block.settings.custom_image != blank -%}
                          {% if settings.images_loading != 'disable' %}
                            <figure class="lazy-image lazy-image--{{ settings.images_loading }}" style="--desktop-asp-rat: {{ block.settings.custom_image.aspect_ratio }}">
                          {% endif %}
                            <img srcset="{%- if block.settings.custom_image.width >= 165 -%}{{ block.settings.custom_image | image_url: width: 165 }} 165w,{%- endif -%}
                                {%- if block.settings.custom_image.width >= 375 -%}{{ block.settings.custom_image | image_url: width: 375 }} 375w,{%- endif -%}
                                {%- if block.settings.custom_image.width >= 550 -%}{{ block.settings.custom_image | image_url: width: 550 }} 550w,{%- endif -%}
                                {%- if block.settings.custom_image.width >= 750 -%}{{ block.settings.custom_image | image_url: width: 750 }} 750w,{%- endif -%}
                                {%- if block.settings.custom_image.width >= 1100 -%}{{ block.settings.custom_image | image_url: width: 1100 }} 1100w,{%- endif -%}
                                {%- if block.settings.custom_image.width >= 1500 -%}{{ block.settings.custom_image | image_url: width: 1500 }} 1500w,{%- endif -%}
                                {%- if block.settings.custom_image.width >= 2200 -%}{{ block.settings.custom_image | image_url: width: 2200 }} 2200w,{%- endif -%}
                                {%- if block.settings.custom_image.width >= 3000 -%}{{ block.settings.custom_image | image_url: width: 3000 }} 3000w,{%- endif -%}
                                {{ block.settings.custom_image | image_url }} {{ block.settings.custom_image.width }}w" src="{{ block.settings.custom_image | image_url: width: 1700 }}"
                                sizes="(min-width: {{ settings_page_width }}) {{ settings_page_width }}, 100vw"
                                alt="{{ block.settings.collection.title | escape }}" 
                                src="{{ block.settings.collection | image_url: width: 1500 }}"
                                {% if settings.images_loading != 'disable' %}
                                  onload="this.parentNode.classList.add('lazyloaded');"
                                  loading="lazy"
                                {% else %}
                                  {% if lazy_load %}loading="lazy"{% endif %}
                                {% endif %}
                                draggable="false"
                                width="{{ block.settings.custom_image.width }}" 
                                height="{{ block.settings.custom_image.height }}" 
                                class="section-featured-collection__image"
                                style="--media-focal-point: {{ block.settings.custom_image.presentation.focal_point }};">
                            {% if settings.images_loading != 'disable' %}
                              {% if settings.images_loading != 'fade_scale' %}
                              <div class="lazy-image__preloader lazy-image__preloader--full lazy-image__preloader-{{ settings.images_loading }}" aria-hidden="true">
                                <img 
                                alt="{{ block.settings.custom_image.alt | escape }}" 
                                src="{{ block.settings.custom_image | image_url: width: 10 }}"
                                draggable="false"
                                width="10" 
                                height="10">
                              </div>
                              {% endif %}
                          </figure>
                          {% endif %}
                        {%- else -%}   
                            {%- if block.settings.collection.featured_image -%}
                              {% if settings.images_loading != 'disable' %}
                                <figure class="lazy-image lazy-image--{{ settings.images_loading }}" style="--desktop-asp-rat: {{ block.settings.collection.featured_image.aspect_ratio }}">
                              {% endif %}
                                <img srcset="{%- if block.settings.collection.featured_image.width >= 165 -%}{{ block.settings.collection.featured_image | image_url: width: 165 }} 165w,{%- endif -%}
                                    {%- if block.settings.collection.featured_image.width >= 375 -%}{{ block.settings.collection.featured_image | image_url: width: 375 }} 375w,{%- endif -%}
                                    {%- if block.settings.collection.featured_image.width >= 550 -%}{{ block.settings.collection.featured_image | image_url: width: 550 }} 550w,{%- endif -%}
                                    {%- if block.settings.collection.featured_image.width >= 750 -%}{{ block.settings.collection.featured_image | image_url: width: 750 }} 750w,{%- endif -%}
                                    {%- if block.settings.collection.featured_image.width >= 1100 -%}{{ block.settings.collection.featured_image | image_url: width: 1100 }} 1100w,{%- endif -%}
                                    {%- if block.settings.collection.featured_image.width >= 1500 -%}{{ block.settings.collection.featured_image | image_url: width: 1500 }} 1500w,{%- endif -%}
                                    {%- if block.settings.collection.featured_image.width >= 2200 -%}{{ block.settings.collection.featured_image | image_url: width: 2200 }} 2200w,{%- endif -%}
                                    {%- if block.settings.collection.featured_image.width >= 3000 -%}{{ block.settings.collection.featured_image | image_url: width: 3000 }} 3000w,{%- endif -%}
                                    {{ block.settings.collection.featured_image | image_url }} {{ block.settings.collection.featured_image.width }}w" 
                                    src="{{ block.settings.collection.featured_image | image_url: width: 1700 }}"
                                    sizes="(min-width: {{ settings_page_width }}) {{ settings_page_width }}, 100vw"
                                    alt="{{ block.settings.collection.title | escape }}" 
                                    src="{{ block.settings.collection | image_url: width: 1500 }}"
                                    {% if settings.images_loading != 'disable' %}
                                      onload="this.parentNode.classList.add('lazyloaded');"
                                      loading="lazy"
                                    {% else %}
                                      {% if lazy_load %}loading="lazy"{% endif %}
                                    {% endif %}
                                    draggable="false"
                                    width="{{ block.settings.collection.featured_image.width }}" 
                                    height="{{ block.settings.collection.featured_image.height }}" 
                                    class="section-featured-collection__image">
                                {% if settings.images_loading != 'disable' %}
                                  {% if settings.images_loading != 'fade_scale' %}
                                  <div class="lazy-image__preloader lazy-image__preloader--full lazy-image__preloader-{{ settings.images_loading }}" aria-hidden="true">
                                    <img 
                                    alt="{{ block.settings.collection.featured_image.alt | escape }}" 
                                    src="{{ block.settings.collection.featured_image | image_url: width: 10 }}"
                                    draggable="false"
                                    width="10" 
                                    height="10">
                                  </div>
                                  {% endif %}
                              </figure>
                              {% endif %}
                            {%- else -%}
                                {{ 'collection-1' | placeholder_svg_tag: 'placeholder-svg' }}
                            {%- endif -%}
                        {%- endif -%} 
                        {% comment %}theme-check-enable ImgLazyLoading{% endcomment %}
                    </div>
                {%- endunless -%}
                <div class="section-featured-collection__products featured-collection-with-product-grid__products" id="ProductGridContainer--{{ block.id }}">
                    {% if block.settings.collection != blank %}
                        {%  for collection in collections %}
                        {% if collection.id == block.settings.collection.id %}
                            {%- paginate collection.products by section.settings.items_per_batch -%}
                                <div class="mobile-grid-{{ mobile_layout }} product-labels__container product-labels__container--{{ columns }} {% if block.settings.collection.products.size == 1 %}border-none{% endif %}" 
                                    data-count="{{ columns }}" data-count-mobile="{{ mobile_layout }}" id="product-grid--{{ block.id }}" data-id="{{ block.id }}">
                                    {% for item in collection.products %}
                                        {% assign lazy_load = false %}
                                        {%- if forloop.index > 2 -%}
                                        {%- assign lazy_load = true -%}
                                        {%- endif -%}
                                        <div class="product-labels__item-wrapper featured-collection-with-product-grid__item-wrapper 
                                        {% if forloop.last and settings.show_lines %}last-desktop{% endif %} 
                                        {% if forloop.last and settings.show_lines and forloop.length >= mobile_layout %}last-mobile{% endif %}">
                                        <div class="product-labels__item multicolumn__item featured-collection-with-product-grid__item">
                                            {% render 'card-product',
                                            card_product: item,
                                            image_ratio: section.settings.card_image_ratio,
                                            show_vendor: section.settings.show_vendor,
                                            show_rating: section.settings.show_rating,
                                            hover_behavior: section.settings.hover_behavior,
                                            show_bullets: section.settings.show_bullets,
                                            lazy_load: lazy_load,
                                            enable_quick_view: section.settings.enable_quick_view,
                                            enable_add_to_cart: section.settings.enable_add_to_cart,
                                            price_position: section.settings.price_position,
                                            max_product_name_characters: section.settings.max_product_name_characters,
                                            show_short_description: section.settings.show_short_description,
                                            enable_color_pills: section.settings.enable_color_pills,
                                            show_pickup_availability: section.settings.show_pickup_availability
                                            %}
                                        </div>
                                        </div>
                                    {% endfor %}
                                </div>
                                {%- if paginate.pages > 1 -%}
                                    {% render 'pagination', paginate: paginate, anchor: '', infinite_scroll: section.settings.infinite_scroll, disable_pagination: true %}
                                {%- endif -%}
                            {%- endpaginate -%}
                            {% endif %}
                        {% endfor %}
                    {% else %}
                        <div class="mobile-grid-{{ mobile_layout }} product-labels__container product-labels__container--{{ columns }} {% if block.settings.collection.products.size == 1 %}border-none{% endif %}" 
                            data-count="{{ columns }}" data-count-mobile="{{ mobile_layout }}">
                              {% for item in (1..limit_placeholder) %}
                                {% assign lazy_load = false %}
                                {%- if forloop.index > 2 -%}
                                  {%- assign lazy_load = true -%}
                                {%- endif -%}
                                {%- assign placeholder_index =  forloop.index -%}
                                {%- if forloop.index > 6 -%}
                                    {%- assign placeholder_index =  '6' -%}
                                {%- endif -%}
                                <div class="product-labels__item-wrapper featured-collection-with-product-grid__item-wrapper 
                                  {% if forloop.last and settings.show_lines %}last-desktop{% endif %} 
                                  {% if forloop.last and settings.show_lines and forloop.length >= mobile_layout %}last-mobile{% endif %}">
                                  <div class="product-labels__item multicolumn__item featured-collection-with-product-grid__item">
                                    {% render 'product-placeholder',
                                      index_placeholder: placeholder_index,
                                      image_ratio: section.settings.card_image_ratio,
                                      show_vendor: section.settings.show_vendor,
                                      price_position: section.settings.price_position
                                      %}
                                  </div>
                                </div>
                              {% endfor %}
                        </div>
                    {% endif %}
                </div>
            </div>
        {%- endfor -%}
    </div>

</div>
{% endif %}

{% schema %}
{
    "name": "t:sections.featured-collection-tabs.name",
    "tag": "section",
    "class": "multicolumn icons_with_text section-tabs section-collection-tabs",
    "max_blocks": 5,
    "settings": [
        {
            "type": "range",
            "id": "items_per_batch",
            "min": 2,
            "max": 50,
            "step": 1,
            "label": "t:sections.featured-collection-with-product-grid.settings.items_per_batch.label",
            "default": 8
        },
        {
          "type": "select",
          "id": "infinite_scroll",
          "options": [
            {
              "value": "disable",
              "label": "t:sections.all.infinite_scroll.options__1.label"
            },
            {
              "value": "button",
              "label": "t:sections.all.infinite_scroll.options__2.label"
            }
          ],
          "default": "button",
          "label": "t:sections.all.infinite_scroll.label"
        },
        {
            "type": "select",
            "id": "media_style",
            "options": [
              {
                "value": "show",
                "label": "t:sections.all.labels.media_style.options__1.label"
              }, 
              {
                "value": "hide",
                "label": "t:sections.all.labels.media_style.options__2.label"
              }, 
              {
                "value": "show_on_desktop",
                "label": "t:sections.all.labels.media_style.options__3.label"
              }
            ],
            "default": "show",
            "label": "t:sections.all.labels.media_style.label"
        },
        {
            "type": "text",
            "id": "subheading",
            "label": "t:sections.all.labels.subheading.label"
        },
        {
            "type": "text",
            "id": "heading",
            "label": "t:sections.all.labels.heading.label",
            "info": "t:sections.all.labels.heading.info"
        },
        {
            "type": "select",
            "id": "tab_heading_size",
            "label": "t:sections.all.size.collapsible_tabs_size.label",
            "options": [
              {
                "value": "default",
                "label": "t:sections.all.size.options__1.label"
              },
              {
                  "value": "heading_xs",
                  "label": "t:sections.all.size.options__10.label"
              },
              {
                  "value": "heading_xs_uppercase",
                  "label": "t:sections.all.size.options__11.label"
              },
              {
                  "value": "body_l",
                  "label": "t:sections.all.size.options__12.label"
              },
              {
                  "value": "body_l_uppercase",
                  "label": "t:sections.all.size.options__13.label"
              },
              {
                  "value": "body_m",
                  "label": "t:sections.all.size.options__14.label"
              }, 
              {
                  "value": "body_m_uppercase",
                  "label": "t:sections.all.size.options__15.label"
              }, 
              {
                  "value": "body_s",
                  "label": "t:sections.all.size.options__16.label"
              }, 
              {
                  "value": "body_s_uppercase",
                  "label": "t:sections.all.size.options__17.label"
              }, 
              {
                  "value": "label_font",
                  "label": "t:sections.all.size.options__18.label"
              },
              {
                  "value": "label_font_uppercase",
                  "label": "t:sections.all.size.options__19.label"
              }
            ],
            "default": "default"
        },
        {
            "type": "checkbox",
            "id": "center_text",
            "default": true,
            "label": "t:sections.all.alignment.center_text.label"
        },
        {
            "type": "select",
            "id": "max_columns",
            "options": [
              {
                "value": "2",
                "label": "t:sections.all.desktop_layout.options__11.label"
              },
              {
                "value": "3",
                "label": "t:sections.all.desktop_layout.options__12.label"
              },
              {
                "value": "4",
                "label": "t:sections.all.desktop_layout.options__13.label"
              }
            ],
            "default": "4",
            "label": "t:sections.all.desktop_layout.max_columns.label"
        },
        {
            "type": "radio",
            "id": "mobile_layout",
            "options": [
              {
                "value": "1",
                "label": "t:sections.all.mobile_layout.options__1.label"
              },
              {
                "value": "2",
                "label": "t:sections.all.mobile_layout.options__2.label"
              }
            ],
            "default": "2",
            "label": "t:sections.all.mobile_layout.label"
        },
        {
            "type": "select",
            "id": "image_ratio",
